<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="historical">

    <class
            name="minestar.platform.domain.metadata.ClassFact"
            table="R_CLASS_FACT"
    >
        <meta attribute="table">
            tablespace=MW_USR
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_USR
        </meta>
        <meta attribute="index">
            name=R_CLASS_FACT_IND
            columns=CLASS_NAME
            unique=true
            tablespace=MW_USR
        </meta>
        <id
                name="OID"
                type="java.lang.Long"
                column="OID"
                access="field"
        >
            <generator class="com.mincom.env.base.persistence.IDGenerator"/>
        </id>

        <property
                name="className"
                column="CLASS_NAME"
                type="java.lang.String"
                length="255"
        >
        </property>
        <property
                name="schemaName"
                column="SCHEMA_NAME"
                type="java.lang.String"
                length="30"
        >
        </property>
        <property
                name="viewName"
                column="VIEW_NAME"
                type="java.lang.String"
                length="30"
        >
        </property>
        <property
                name="tableName"
                column="TABLE_NAME"
                type="java.lang.String"
                length="30"
        >
        </property>
        <property
                name="universeName"
                column="UNIVERSE_NAME"
                type="java.lang.String"
                length="30"
        >
        </property>
        <property
                name="universeClassName"
                column="UNIVERSE_CLASS_NAME"
                type="java.lang.String"
                length="128"
        >
        </property>
        <property
                name="xmlFileLocation"
                column="XML_FILE_LOCATION"
                type="java.lang.String"
                length="254"
        >
        </property>
        <property
                name="xmlFileName"
                column="XML_FILENAME"
                type="java.lang.String"
                length="64"
        >
        </property>
        <property
                name="description"
                column="DESCRIPTION"
                type="java.lang.String"
                length="4000"
        >
        </property>

        <set name="propertyFacts" cascade="all-delete-orphan">
            <key column="CLASS_FACT_OID"/>
            <one-to-many class="minestar.platform.domain.metadata.PropertyFact"/>
        </set>

    </class>

    <class
            name="minestar.platform.domain.metadata.PropertyFact"
            proxy="minestar.platform.domain.metadata.PropertyFact"
            table="R_PROPERTY_FACT"
    >
        <meta attribute="table">
            tablespace=MW_USR
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_USR
        </meta>
        <meta attribute="index">
            name=R_PROPERTY_FACT_IND
            columns=PROPERTY_NAME
            tablespace=MW_USR
        </meta>
        <id
                name="OID"
                type="java.lang.Long"
                column="OID"
                access="field"
        >
            <generator class="com.mincom.env.base.persistence.IDGenerator"/>
        </id>

        <property
                name="propertyName"
                column="PROPERTY_NAME"
                type="java.lang.String"
                length="50"
        >
        </property>
        <property
                name="propertyType"
                column="PROPERTY_TYPE"
                type="java.lang.String"
                length="34"
        >
        </property>
        <property
                name="propertyDataType"
                type="java.lang.String"
                column="DATATYPE"
                length="255"
        >
        </property>
        <property
                name="propertyAlias"
                column="PROPERTY_ALIAS"
                type="java.lang.String"
                length="50"
        >
        </property>
        <property
                name="viewColumnName"
                column="VIEW_COLUMN_NAME"
                type="java.lang.String"
                length="50"
        >
        </property>
        <property
                name="columnName"
                column="COLUMN_NAME"
                type="java.lang.String"
                length="255"
        >
        </property>
        <property
                name="isNullable"
                type="java.lang.Boolean"
                column="IS_NULLABLE"
        >
        </property>
        <property
                name="universeName"
                column="UNIVERSE_NAME"
                type="java.lang.String"
                length="30"
        >
        </property>
        <property
                name="universeObjectName"
                column="UNIVERSE_OBJECT_NAME"
                type="java.lang.String"
                length="128"
        >
        </property>
        <property
                name="isReportPrompt"
                type="java.lang.Boolean"
                column="IS_RPTPROMPT"
        >
        </property>
        <property
                name="dbDataTypeName"
                column="DB_DATATYPE_NAME"
                type="java.lang.String"
                length="34"
        >
        </property>
        <property
                name="dbDataTypeLength"
                column="DB_DATATYPE_LENGTH"
                type="java.lang.Integer"
        >
        </property>
        <property
                name="dbDataTypePrecision"
                column="DB_DATATYPE_PRECISION"
                type="java.lang.Integer"
        >
        </property>

        <!-- Refact'd from PropertyUnit: -->

        <property
                name="unitName"
                column="UNIT_NAME"
                type="java.lang.String"
                length="64"
        >
        </property>

        <property
                name="unitTypeName"
                column="UNIT_TYPE_NAME"
                type="java.lang.String"
                length="32"
        >
        </property>

        <property
                name="unitFullName"
                column="UNIT_FULL_NAME"
                type="java.lang.String"
                length="32"
        >
        </property>
        <property
                name="unitAbbrName"
                column="UNIT_ABBR_NAME"
                type="java.lang.String"
                length="32"
        >
        </property>
        <property
                name="precision"
                column="PRECISION"
                type="java.lang.Integer"
        >
        </property>
        <property
                name="fractionalMultiplier"
                column="FRACTIONAL_MULTIPLIER"
                type="java.lang.Double"
        >
        </property>
        <property
                name="offsetOperator"
                column="OFFSET_OPERATOR"
                type="java.lang.Double"
        >
        </property>
        <property
                name="conversionMethod"
                column="CONVERSION_METHOD"
                type="java.lang.String"
                length="32"
        >
        </property>
        <property
                name="boFormatMask"
                column="BO_FORMAT_MASK"
                type="java.lang.String"
                length="32"
        >
        </property>
        <property
                name="guiFormatMask"
                column="GUI_FORMAT_MASK"
                type="java.lang.String"
                length="32"
        >
        </property>
        <property
                name="dbFormatMask"
                column="DB_FORMAT_MASK"
                type="java.lang.String"
                length="32"
        >
        </property>
        <property
                name="description"
                column="DESCRIPTION"
                type="java.lang.String"
                length="4000"
        >
        </property>

    </class>
</hibernate-mapping>
