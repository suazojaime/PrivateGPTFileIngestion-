<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="model">

    <class name="minestar.health.api.definition.MachineClassMeasureConfig" table="MACHINE_CLASS_MEASURE_CONFIG">

        <meta attribute="noDatabaseMetadata" inherit="false">true</meta>

        <meta attribute="view-include">false</meta>
        <meta attribute="view-ignore">true</meta>

        <meta attribute="table">
            tablespace=MW_OBJ
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXOBJ
        </meta>

        <cache usage="read-write" region="machineClassMeasureConfig"/>

        <composite-id>

            <meta attribute="view-include">false</meta>
            <meta attribute="view-ignore">true</meta>

            <key-many-to-one name="machineClassRef" class="minestar.pitlink.domain.machine.MachineClassReference" column="MACHINE_CLASS_OID" lazy="false">
                <meta attribute="alias">machineClass</meta>
                <meta attribute="view-include">false</meta>
                <meta attribute="view-ignore">true</meta>
            </key-many-to-one>
            <key-many-to-one name="measure" class="minestar.health.api.definition.Measure" column="MEASURE_OID" lazy="false">
                <meta attribute="view-include">false</meta>
                <meta attribute="view-ignore">true</meta>
            </key-many-to-one>

        </composite-id>
    </class>
</hibernate-mapping>
