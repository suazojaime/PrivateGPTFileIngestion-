<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="historical">

    <class
            name="minestar.platform.domain.metadata.RUnitDefine"
            table="R_UNIT_DEFINE"
            >
        <meta attribute="table">
            tablespace=MW_ECF
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXENT
        </meta>
        <id
                name="name"
                type="java.lang.String"
                column="NAME"
                >
            <generator class="assigned" />
        </id>

        <property
                name="clazz"
                type="java.lang.String"
                column="CLASS"
                length="32"
                >
        </property>
        <property
                name="base"
                type="java.lang.String"
                column="BASE"
                length="32"
                >
        </property>
        <property
                name="fullName"
                type="java.lang.String"
                column="FULLNAME"
                length="128"
                >
        </property>
        <property
                name="abbrName"
                type="java.lang.String"
                column="ABBRNAME"
                length="32"
                >
        </property>
        <property
                name="precision"
                type="java.lang.Short"
                column="PRECISION"
                >
        </property>
        <property
                name="scale"
                type="java.lang.Double"
                column="SCALE"
                >
        </property>
        <property
                name="offset"
                type="java.lang.Double"
                column="OFFSET"
                >
        </property>
        <property
                name="convtype"
                type="java.lang.String"
                column="CONVTYPE"
                length="32"
                >
        </property>

    </class>
</hibernate-mapping>
