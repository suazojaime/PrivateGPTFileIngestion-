<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:widget="http://www.minestar.au.cat.com/namespace/widget"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.minestar.au.cat.com/namespace/widget http://www.minestar.au.cat.com/namespace/widget.xsd
       ">


    <widget:areaXYChart
            id="truckDumpingActivityAreaXY"
            dataSet="hoursInShiftDataSet"
            dataSource="hourlyTonsMined"
            labelX="Shift Hour"
            title="Payload in Shift"
            maxItemsPerChart="12"
            minValue="0"
            maxValue="16000"
            width="1500"
            height="650"
            domain="items"
            backgroundColour="graphBackground"
            multiChartGroupMode="region"
            />


    <widget:verticalLineChart id="tonsMinedPerHourInShiftLineChart" dataSet="hoursInShiftDataSet"
                              dataSource="hourlyTonsMined"
                              labelX="Shift Hour" title="Payload in Shift" maxItemsPerChart="12"
                              minValue="0" maxValue="16000"
                              width="1500" height="650" domain="properties"
                              backgroundColour="graphBackground" multiChartGroupMode="region">
        <widget:intervalRef ref="tonsMinedPerHourInShiftTarget"/>
    </widget:verticalLineChart>


</beans>
