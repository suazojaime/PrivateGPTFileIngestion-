<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       ">

    <bean id="siteAwarenessService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.pitlink.service.awareness.SiteAwarenessService"/>
    </bean>

    <bean id="fieldCommunicationsService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.pitlink.service.communications.FieldCommunicationsService"/>
    </bean>

    <bean id="spatialIndexingService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.pitlink.service.awareness.version2.SpatialIndexingService"/>
    </bean>

    <bean id="gwmDuplicatesService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.pitlink.service.tools.GwmDuplicatesService"/>
    </bean>

    <bean id="mineModelMachineStatusService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.minemodelupdate.machinestatus.MineModelMachineStatusService"/>
    </bean>

    <bean id="autonomousZoneEntryService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.autonomy.service.zoneentry.AutonomousZoneEntryService"/>
    </bean>
    
    <bean id="telemetryService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.health.api.telemetry.TelemetryService"/>
    </bean>
    <bean id="definitionService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.health.api.definition.DefinitionService"/>
    </bean>
    <bean id="healthDataVisualizationService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.pitlink.service.health.HealthDataVisualizationService"/>
    </bean>
</beans>
