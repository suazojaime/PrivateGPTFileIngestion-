<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="2919b69c-be5d-4e39-9135-42310444df85" author="MineStar" failOnError="false">
        <preConditions onFailMessage="Ignored AddColumn for EDITABLE to table LANE as table does not exist or column already exists." onFail="MARK_RAN">
            <tableExists tableName="LANE"/>
            <not>
                <columnExists tableName="LANE" columnName="EDITABLE"/>
            </not>
        </preConditions>
        <comment>AHS-2985: AddColumn LANE.EDITABLE</comment>
        <addColumn tableName="LANE">
            <column name="EDITABLE" type="BOOLEAN" defaultValueBoolean="true" remarks="Is this lane editable?">
                <constraints nullable="false"/>
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>
