<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="roadLayer" class="minestar.mine.application.graph.layer.RoadLayer" lazy-init="true"/>
    <bean id="roadHaulageLayer" class="minestar.mine.application.graph.layer.RoadLayer" lazy-init="true"/>
    <bean id="waypointLayer" class="minestar.mine.application.graph.layer.WaypointLayer" lazy-init="true"/>
    <bean id="locationLayer" class="minestar.mine.application.graph.layer.LocationLayer" lazy-init="true"/>
    <bean id="truckLayer" class="minestar.mine.application.graph.layer.TruckLayer" lazy-init="true"/>
    <bean id="personnelLayer" class="minestar.mine.application.graph.layer.personnel.PersonnelLayer" lazy-init="true"/>
    <bean id="processorLayer" class="minestar.mine.application.graph.layer.processor.ProcessorLayer" lazy-init="true"/>
    <bean id="loadingToolLayer" class="minestar.mine.application.graph.layer.LoadingToolLayer" lazy-init="true"/>
    <bean id="secondaryBreakageUnitLayer" class="minestar.mine.application.graph.layer.SecondaryBreakageUnitLayer" lazy-init="true"/>
    <bean id="fuelbayLayer" class="minestar.mine.application.graph.layer.FuelBayLayer" lazy-init="true"/>
    <bean id="scaleLayer" class="minestar.mine.application.graph.layer.PayloadServiceLayer" lazy-init="true"/>
    <bean id="infrastructureLayer" class="minestar.mine.application.graph.layer.InfrastructureLayer" lazy-init="true"/>
    <bean id="gradeBlockLayer" class="minestar.mine.application.graph.layer.GradeBlockLayer" lazy-init="true"/>
    <bean id="auxiliaryLayer" class="minestar.mine.application.graph.layer.AuxiliaryLayer" lazy-init="true"/>
    <bean id="nonWaterTruckAuxiliaryLayer" class="minestar.mine.application.graph.layer.NonWaterTruckAuxiliaryLayer" lazy-init="true"/>
    <bean id="waterTruckLayer" class="minestar.mine.application.graph.layer.WaterTruckLayer" lazy-init="true"/>
    <bean id="dxfLayer" class="minestar.mine.application.graph.dxf.DxfLayer" lazy-init="true"/>
    <bean id="dxfEditableLayer" class="minestar.mine.application.graph.dxf.DxfEditableLayer" lazy-init="true"/>
    <bean id="dxfCalibrationLayer" class="minestar.mine.application.graph.dxf.DxfCalibrationLayer" lazy-init="true"/>
    <bean id="surfaceAreaLayer" class="minestar.mine.application.graph.layer.surface.SurfaceAreaLayer" lazy-init="true"/>
    <bean id="surfaceLayer" class="minestar.mine.application.graph.dxf.SurfaceLayer" lazy-init="true"/>
    <bean id="areaMapLayer" class="minestar.mine.application.graph.layer.underground.areamap.AreaMapLayer"
          lazy-init="true"/>
    <bean id="orePassLayer" class="minestar.mine.application.graph.layer.orepass.OrePassLayer" lazy-init="true">
        <constructor-arg ref="locationLayer"/>
    </bean>
    <bean id="drawPointLayer" class="minestar.mine.application.graph.layer.drawpoint.DrawPointLayer" lazy-init="true">
        <constructor-arg ref="locationLayer"/>
    </bean>
    <bean id="positionOfInterestLayer" class="minestar.mine.application.graph.layer.underground.PositionOfInterestLayer" lazy-init="true"/>
    <bean id="rfrNodeLayer" class="minestar.mine.application.graph.layer.underground.rfrnode.RFRNodeLayer" lazy-init="true"/>
    <bean id="chuteLayer" class="minestar.mine.application.graph.layer.chute.ChuteLayer" lazy-init="true"/>
    <bean id="loadingBayLayer" class="minestar.mine.application.graph.layer.loadingbay.LoadingBayLayer" lazy-init="true"/>
    <bean id="rockBreakerLayer" class="minestar.mine.application.graph.layer.RockBreakerLayer" lazy-init="true"/>
    <bean id="zoneLayer" class="minestar.mine.application.graph.layer.zone.ZoneLayer" lazy-init="true"/>
    <bean id="anonymousMachineLayer" class="minestar.mine.application.graph.layer.machine.AnonymousMachineLayer" lazy-init="true"/>
    <bean id="dumpGoalLayer"
          class="minestar.mine.application.graph.layer.underground.dumpgoal.DumpGoalLayer"
          lazy-init="true">
        <property name="orePassLayer" ref="orePassLayer"/>
    </bean>
    <bean id="loadGoalLayer" class="minestar.mine.application.graph.layer.underground.loadgoal.LoadGoalLayer" lazy-init="true">
        <property name="drawPointLayer" ref="drawPointLayer"/>
    </bean>
    <bean id="inversionGoalLayer" class="minestar.mine.application.graph.layer.underground.inversiongoal.InversionGoalLayer" lazy-init="true"/>
    <bean id="routeLayer" class="minestar.mine.application.graph.layer.underground.route.RouteLayer" lazy-init="true"/>
    <bean id="aimsLayer" class="minestar.mine.application.graph.layer.underground.aims.AIMSLayer" lazy-init="true"/>
    <bean id="pinLayer" class="minestar.mine.application.graph.layer.underground.pin.PinLayer" lazy-init="true"/>
    <bean id="signLayer" class="minestar.mine.application.graph.layer.underground.sign.SignLayer" lazy-init="true"/>

    <bean id="joinedUndergroundAreaLayer"
          class="minestar.mine.application.graph.layer.underground.area.JoinedUndergroundAreaLayer"
          lazy-init="true"/>
    <bean id="drivingBehaviourLayer" class="minestar.autonomy.presentation.graph.drive.DriveBehaviorLayer" lazy-init="true"/>

    <bean id="gradeBlockEditableLayer" class="minestar.mine.application.graph.layer.GradeBlockEditableLayer" lazy-init="true">
        <constructor-arg ref="locationEditableLayer"/>
    </bean>
    <bean id="roadEditableLayer" class="minestar.mine.application.graph.layer.RoadEditableLayer" lazy-init="true"/>
    <bean id="waypointEditableLayer" class="minestar.mine.application.graph.layer.WaypointEditableLayer" lazy-init="true"/>
    <bean id="machineTrailLayer" class="minestar.mine.presentation.graph.trail.MachineTrailLayer" lazy-init="true"/>
    <bean id="locationEditableLayer" class="minestar.mine.application.graph.layer.LocationEditableLayer" lazy-init="true"/>
    <bean id="blastholeDrillLayer" class="minestar.mine.application.graph.layer.BlastholeDrillLayer" lazy-init="true"/>
    <bean id="draglineLayer" class="minestar.mine.application.graph.layer.DraglineLayer" lazy-init="true"/>
    <bean id="shutdownMachineEditableLayer" class="minestar.mine.application.graph.layer.machine.ShutdownMachineEditableLayer" lazy-init="true"/>
    <bean id="startedMachineEditableLayer" class="minestar.mine.application.graph.layer.machine.StartedMachineEditableLayer" lazy-init="true"/>
    <bean id="areaMapEditableLayer"
          class="minestar.mine.application.graph.layer.underground.areamap.AreaMapEditableLayer"
          lazy-init="true"/>
    <bean id="orePassEditableLayer" class="minestar.mine.application.graph.layer.orepass.OrePassEditableLayer" lazy-init="true">
        <constructor-arg ref="locationEditableLayer"/>
    </bean>
    <bean id="drawPointEditableLayer" class="minestar.mine.application.graph.layer.drawpoint.DrawPointEditableLayer" lazy-init="true">
        <constructor-arg ref="locationEditableLayer"/>
    </bean>
    <bean id="positionOfInterestEditableLayer" class="minestar.mine.application.graph.layer.underground.PositionOfInterestEditableLayer" lazy-init="true"/>
    <bean id="chuteEditableLayer" class="minestar.mine.application.graph.layer.chute.ChuteEditableLayer" lazy-init="true">
        <constructor-arg ref="locationEditableLayer"/>
        <constructor-arg ref="waypointEditableLayer"/>
    </bean>
    <bean id="processorEditableLayer" class="minestar.mine.application.graph.layer.processor.ProcessorEditableLayer" lazy-init="true">
        <constructor-arg ref="locationEditableLayer"/>
        <constructor-arg ref="waypointEditableLayer"/>
    </bean>
    <bean id="loadingBayEditableLayer" class="minestar.mine.application.graph.layer.loadingbay.LoadingBayEditableLayer" lazy-init="true">
        <constructor-arg ref="locationEditableLayer"/>
        <constructor-arg ref="waypointEditableLayer"/>
    </bean>
    <bean id="rfrNodeEditableLayer" class="minestar.mine.application.graph.layer.underground.rfrnode.RFRNodeEditableLayer" lazy-init="true"/>
    <bean id="infrastructureEditableLayer" class="minestar.mine.application.graph.layer.infrastructure.InfrastructureEditableLayer" lazy-init="true"/>
    <bean id="zoneEditableLayer" class="minestar.mine.application.graph.layer.zone.ZoneEditableLayer" lazy-init="true"/>
    <bean id="dumpGoalEditableLayer"
          class="minestar.mine.application.graph.layer.underground.dumpgoal.DumpGoalEditableLayer"
          lazy-init="true">
        <property name="orePassLayer" ref="orePassEditableLayer"/>
    </bean>
    <bean id="loadGoalEditableLayer" class="minestar.mine.application.graph.layer.underground.loadgoal.LoadGoalEditableLayer" lazy-init="true">
        <property name="drawPointLayer" ref="drawPointEditableLayer"/>
    </bean>
    <bean id="inversionGoalEditableLayer" class="minestar.mine.application.graph.layer.underground.inversiongoal.InversionGoalEditableLayer" lazy-init="true"/>
    <bean id="routeEditableLayer" class="minestar.mine.application.graph.layer.underground.route.RouteEditableLayer" lazy-init="true"/>
    <bean id="aimsEditableLayer" class="minestar.mine.application.graph.layer.underground.aims.AIMSEditableLayer" lazy-init="true"/>
    <bean id="joinedUndergroundAreaEditableLayer"
          class="minestar.mine.application.graph.layer.underground.area.JoinedUndergroundAreaEditableLayer"
          lazy-init="true"/>
    <bean id="wallEditableLayer" class="minestar.mine.application.graph.layer.underground.wall.WallEditableLayer" lazy-init="true"/>
    <bean id="wallLayer" class="minestar.mine.application.graph.layer.underground.wall.WallLayer" lazy-init="true"/>
    <bean id="drivingBehaviourEditableLayer" class="minestar.autonomy.presentation.graph.drive.DriveBehaviorEditableLayer" lazy-init="true"/>
    <!--<bean id="laneLayer" class="minestar.mine.application.graph.layer.lane.LaneLayer" lazy-init="true"/>-->
    <bean id="laneEditableLayer" class="minestar.mine.application.graph.layer.lane.LaneEditableLayer" lazy-init="true"/>
    <bean id="pinEditableLayer" class="minestar.mine.application.graph.layer.underground.pin.PinEditableLayer" lazy-init="true"/>
    <bean id="signEditableLayer" class="minestar.mine.application.graph.layer.underground.sign.SignEditableLayer" lazy-init="true"/>

</beans>
