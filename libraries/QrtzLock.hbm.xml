<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="model">

    <class
            name="minestar.external.domain.QrtzLock"
            table="QRTZ_LOCKS"
    >
        <meta attribute="table">
            tablespace=MW_OBJ
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXOBJ
        </meta>
        <meta attribute="description">The Quartz timer lock.</meta>


        <id
                name="lockName"
                type="java.lang.String"
                column="LOCK_NAME"
                length="254"
        >
            <meta attribute="description">The lock name</meta>
            <generator class="assigned"/>
        </id>


        <!-- associations -->

    </class>
</hibernate-mapping>
