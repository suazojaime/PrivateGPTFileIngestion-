<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       ">

    <bean id="mineLayoutService" class="minestar.mine.service.minemodel.MineLayoutService" factory-method="getInstance" lazy-init="true"/>

    <bean id="zoneLayoutService" class="minestar.mine.service.minemodel.ZoneLayoutService" factory-method="getInstance" lazy-init="true"/>

    <bean id="safetyCheckService" class="minestar.pitlink.service.safety.SafetyCheckService" factory-method="getInstance" lazy-init="true"/>

    <bean id="shiftChangeService" class="minestar.mine.service.shiftchange.ShiftChangeService" factory-method="getInstance" lazy-init="true"/>

    <bean id="dataInspectionService" class="minestar.mine.service.inspection.DataInspectionService" factory-method="getInstance" lazy-init="true"/>

    <bean id="personnelService" class="minestar.mine.service.personnel.PersonnelService" factory-method="getInstance" lazy-init="true"/>

    <bean id="versionInfoService" class="minestar.mine.service.minemodel.version.VersionInfoService" factory-method="getInstance" lazy-init="true"/>

    <bean id="timeUsageModelManagementService" class="minestar.platform.service.timeusagemodel.TimeUsageModelManagementService" factory-method="getInstance" lazy-init="true"/>

</beans>
