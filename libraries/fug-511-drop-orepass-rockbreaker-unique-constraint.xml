        <rollback>
            <addUniqueConstraint columnNames="ROCK_BREAKER" constraintName="ORE_PASS_ROCK_BREAKER_UK" tableName="ORE_PASS"/>
        </rollback>
    </changeSet>
</databaseChangeLog>
