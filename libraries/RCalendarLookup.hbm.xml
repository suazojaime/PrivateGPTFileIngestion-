<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="historical">

    <class
            name="minestar.platform.domain.lookup.RCalendarLookup"
            table="R_CALENDAR_LOOKUP"
    >
        <meta attribute="table">
            tablespace=MW_ECF
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXENT
        </meta>
        <meta attribute="index">
            name=R_CALENDAR_LOOKUP_IND
            columns=NAME
            unique=false
            tablespace=MW_IDXENT
        </meta>
        <composite-id>
            <key-property
                    name="name"
                    column="NAME"
                    type="java.lang.String"
                    length="32"
            >
                <meta attribute="use-in-equals">true</meta>
            </key-property>
            <key-property
                    name="offset"
                    column="OFFSET"
                    type="java.lang.Long"
            >
                <meta attribute="use-in-equals">true</meta>
            </key-property>
            <key-property
                    name="locale"
                    column="LOCALE"
                    type="java.lang.String"
                    length="32"
            >
                <meta attribute="use-in-equals">true</meta>
            </key-property>
        </composite-id>

        <property
                name="OID"
                column="OID"
                type="java.lang.String"
                length="32"
        >
        </property>
        <property
                name="offsetName"
                column="OFFSET_NAME"
                type="java.lang.String"
                length="80"
        >
        </property>
        <property
                name="system"
                column="SYSTEM"
                type="java.lang.String"
                length="32"
        >
        </property>


    </class>
</hibernate-mapping>
