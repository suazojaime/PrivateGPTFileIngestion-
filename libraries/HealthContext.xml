<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       ">

    <bean id="healthService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.health.service.health.HealthService"/>
    </bean>

    <bean id="definitionService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.health.api.definition.DefinitionService"/>
    </bean>

    <bean id="dataFrameService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.health.api.frame.DataFrameService"/>
    </bean>
    
    <bean id="vimsFileService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.health.api.importer.VimsFileService"/>
    </bean>

    <bean id="healthDataTypeService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.health.service.health.HealthDataTypeService"/>
    </bean>

    <bean id="healthEventService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.health.service.health.HealthEventService"/>
    </bean>


    <bean id="fluidHealthService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.health.service.health.FluidHealthService"/>
    </bean>

    <bean id="classFactService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.platform.service.generation.ClassFactService"/>
    </bean>

    <bean id="telemetryService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.health.api.telemetry.TelemetryService"/>
    </bean>

   <bean id="systemChannelPollingService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.health.api.telemetry.SystemChannelPollingService"/>
    </bean>

    <bean id="healthSegProductionEventService" parent="serviceInstance" lazy-init="true">
        <constructor-arg value="minestar.health.service.health.HealthSegProductionEventService"/>
    </bean>
</beans>
