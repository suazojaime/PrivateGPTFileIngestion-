<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="model" default-access="field">

    <class name="minestar.health.api.definition.ConditionDef" table="CONDITION_DEF">
        <meta attribute="noDatabaseMetadata" inherit="true">true</meta>

        <meta attribute="table">
            tablespace=MW_OBJ
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXOBJ
        </meta>

        <meta attribute="view-create-order">1</meta>

        <cache usage="read-write" region="conditionDef"/>

        <id name="oid" type="long" column="OID">
            <generator class="assigned"/>
        </id>

        <property name="family"
                  type="minestar.health.impl.definition.MeasureFamilyUserType"
                  column="FAMILY"
                  length="12"
                  not-null="true" />

        <property name="protocolConditionID"
                  type="long"
                  column="PROTOCOL_CONDITION_ID"
                  not-null="true" />

        <property name="name" column="NAME" length="64"/>

    </class>
</hibernate-mapping>
