<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field" schema="model">

    <class name="minestar.mine.domain.minemodel.underground.dumpgoal.DumpGoalImpl"
           proxy="minestar.mine.domain.minemodel.underground.dumpgoal.DumpGoal"
           table="DUMP_GOAL"
           dynamic-update="true">

        <meta attribute="alias" inherit="false">DumpGoal</meta>
        <meta attribute="view-master">true</meta>
        <meta attribute="description">
            The approach of a command machine to dump into an ore pass.
        </meta>
        <meta attribute="table">
            tablespace=MW_ENT
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXENT
        </meta>
        <meta attribute="index">
            name=DUMPGOAL_NAME
            unique=true
            columns=DUMP_GOAL_NAME
            tablespace=MW_IDXENT
        </meta>
        <meta attribute="index">
            name=DUMPGOAL_OREPASS
            unique=false
            columns=ORE_PASS
            tablespace=MW_IDXENT
        </meta>

        <id name="OID"
            type="java.lang.Long"
            column="OID">
            <generator class="com.mincom.env.base.persistence.IDGenerator"/>
        </id>

        <property name="modelUpdateVersion"
                  type="java.lang.Long"
                  column="MODEL_UPDATE_VERSION">
            <meta attribute="description">The is a copy of the system wide version number.</meta>
            <meta attribute="universe-type-measure">false</meta>
            <meta attribute="export-marshall" inherit="false">false</meta>
        </property>

        <many-to-one name="routeNodeRef"
                     class="minestar.mine.domain.minemodel.underground.route.RouteNodeReference"
                     column="ROUTE_NODE_OID"
                     lazy="false">
            <meta attribute="description">
                The associated Route Node. This is calculated by the System.
            </meta>
        </many-to-one>

        <property name="name"
                  type="java.lang.String"
                  column="DUMP_GOAL_NAME"
                  length="254"
                  not-null="true">
            <meta attribute="description">The unique, user-entered identifier for this dump goal.</meta>
        </property>

        <property name="active"
                  type="java.lang.Boolean"
                  column="IS_ACTIVE"
                  not-null="true">
            <meta attribute="defaultValue">true</meta>
            <meta attribute="description">
                Whether this dump goal is active or archived.
                (This flag is '1' if it is active and '0' if archived.)
            </meta>
        </property>


        <property name="assignable"
                  type="java.lang.Boolean"
                  column="IS_ASSIGNABLE"
                  not-null="true">
            <meta attribute="defaultValue">true</meta>
            <meta attribute="description">
                Whether this dump goal is assignable.
                (This flag is '1' if it is assignable and '0' if not assignable.)
            </meta>
        </property>


        <many-to-one name="orePass"
                     class="minestar.mine.domain.minemodel.underground.orepass.OrePassReference"
                     column="ORE_PASS"
                     not-null="true"
                     lazy="false">
            <meta attribute="alias">orePass</meta>
            <meta attribute="description">The ore pass that this dump goal is linked to.</meta>
        </many-to-one>

        <property name="position"
                  type="XYZCoordinate">
            <meta attribute="description">The center point of the dump goal.</meta>
            <column name="POSITION_X" not-null="true"/>
            <column name="POSITION_Y" not-null="true"/>
            <column name="POSITION_Z" not-null="true"/>
        </property>

        <property name="direction"
                  column="DIRECTION"
                  not-null="true">
            <meta attribute="description">The direction that the arrow is pointing away from the center point.</meta>
            <meta attribute="unitType">plane angle</meta>
            <type name="quantity">
                <param name="unitType">plane angle</param>
            </type>
        </property>

        <property name="finalPosition"
                  type="XYZCoordinate">
            <meta attribute="description">The end position for the dump macro.</meta>
            <column name="FINAL_POSITION_X"/>
            <column name="FINAL_POSITION_Y"/>
            <column name="FINAL_POSITION_Z"/>
        </property>

        <property name="improvedDumpAutomationEnabled"
                  type="java.lang.Boolean"
                  column="IDA_ENABLED">
            <meta attribute="description">
                Whether the Command's Improved Dump Automation features is enabled for this dump goal
            </meta>
        </property>

        <property name="dumpCeilingHeight"
                  column="DUMP_CEILING_HEIGHT">
            <meta attribute="description">The ceiling height at the dump point.</meta>
            <meta attribute="unitType">length</meta>
            <type name="quantity">
                <param name="unitType">length</param>
            </type>
        </property>

        <property name="tramCeilingHeight"
                  column="TRAM_CEILING_HEIGHT">
            <meta attribute="description">The ceiling height at the tramming zone.</meta>
            <meta attribute="unitType">length</meta>
            <type name="quantity">
                <param name="unitType">length</param>
            </type>
        </property>

        <property name="bundHeight"
                  column="BUND_HEIGHT">
            <meta attribute="description">The height of the bund.</meta>
            <meta attribute="unitType">length</meta>
            <type name="quantity">
                <param name="unitType">length</param>
            </type>
        </property>

        <property name="bundDistance"
                  column="BUND_DISTANCE">
            <meta attribute="description">The distance of the bund from the dump point.</meta>
            <meta attribute="unitType">length</meta>
            <type name="quantity">
                <param name="unitType">length</param>
            </type>
        </property>

        <property name="shakesCount"
                  type="java.lang.Integer"
                  column="SHAKES_COUNT">
            <meta attribute="description">The number of shakes.</meta>
        </property>

    </class>

</hibernate-mapping>
