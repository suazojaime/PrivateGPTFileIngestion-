    <changeSet id="c86295fb-361b-41e4-a893-ec46d62c790b" author="MineStar" failOnError="false">
        <preConditions onFailMessage="Ignored AddPrimaryKey for HISTORIC_REALTIMEKPI_PK to table HISTORIC_REALTIMEKPI as table does not exist or primaryKey already exists." onFail="MARK_RAN">
            <tableExists tableName="HISTORIC_REALTIMEKPI"/>
            <not>
                <primaryKeyExists primaryKeyName="HISTORIC_REALTIMEKPI_PK" tableName="HISTORIC_REALTIMEKPI"/>
            </not>
        </preConditions>
        <comment>FLT-10012: AddPrimaryKey HISTORIC_REALTIMEKPI_PK to HISTORIC_REALTIMEKPI (REALTIMEKPI_OID)</comment>
        <!-- replace this with a custom change -->
        <sql dbms="oracle">DELETE FROM HISTORIC_REALTIMEKPI WHERE ROWID IN (SELECT RID FROM (SELECT ROWID RID, ROW_NUMBER() OVER (PARTITION BY REALTIMEKPI_OID ORDER BY ROWID) RN FROM HISTORIC_REALTIMEKPI) WHERE RN &lt;&gt; 1)</sql>
        <sql dbms="mssql">WITH CTE AS (SELECT ROW_NUMBER() OVER (PARTITION BY REALTIMEKPI_OID ORDER BY %%physloc%%) AS ROW_NUM FROM HISTORIC_REALTIMEKPI) DELETE FROM CTE WHERE ROW_NUM &gt; 1</sql>
        <addPrimaryKey columnNames="REALTIMEKPI_OID" constraintName="HISTORIC_REALTIMEKPI_PK" tableName="HISTORIC_REALTIMEKPI"/>
        <rollback>
            <dropPrimaryKey constraintName="HISTORIC_REALTIMEKPI_PK" tableName="HISTORIC_REALTIMEKPI"/>
        </rollback>
        <modifySql dbms="mssql,h2,postgresql">
            <regExpReplace replace="( USING INDEX)? (ON|TABLESPACE) MW_[A-Z]+" with=""/>
        </modifySql>
    </changeSet>
