<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="minestar.externalreference.domain.LinkSystemEntityReferences" table="LINK_SYSTEM_ENTITY_REFERENCES"
           schema="model">
        <meta attribute="table">
            tablespace=MW_ENT
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXENT
        </meta>
        <meta attribute="description">Linking SystemEntityReferences</meta>
        <id name="OID" type="java.lang.Long" access="field" column="externalSystemEntityReference">
            <generator class="foreign">
                <param name="property">externalReferenceId</param>
            </generator>
        </id>
        <one-to-one
                name="externalReferenceId"
                class="minestar.externalreference.domain.SystemEntityReference"
                access="field"
                constrained="true">
        </one-to-one>
        <many-to-one
                name="minestarReferenceId"
                column="minestarSystemEntityReference"
                not-null="true"
                class="minestar.externalreference.domain.SystemEntityReference">
        </many-to-one>
    </class>
</hibernate-mapping>
