    <changeSet id="5e9ad5f0-b804-4f99-bec3-3b3521883b76" author="MineStar" context="Underground/Underground_Management" failOnError="false">
        <preConditions onFailMessage="Ignored AddPrimaryKey for UG_RESTRICTION_PK to table UG_RESTRICTION as table does not exist or primaryKey already exists." onFail="MARK_RAN">
            <tableExists tableName="UG_RESTRICTION"/>
            <not>
                <primaryKeyExists primaryKeyName="UG_RESTRICTION_PK" tableName="UG_RESTRICTION"/>
            </not>
        </preConditions>
        <comment>FUG-6957: AddPrimaryKey UG_RESTRICTION_PK to UG_RESTRICTION (MACHINE_OID,GOAL_OID,GOAL_NAME,GOAL_TYPE)</comment>
        <sql dbms="oracle">DELETE FROM UG_RESTRICTION WHERE ROWID IN (SELECT RID FROM (SELECT ROWID RID, ROW_NUMBER() OVER (PARTITION BY MACHINE_OID, GOAL_OID, GOAL_NAME, GOAL_TYPE ORDER BY ROWID) RN FROM UG_RESTRICTION) WHERE RN &lt;&gt; 1)</sql>
        <sql dbms="mssql">WITH CTE AS (SELECT ROW_NUMBER() OVER (PARTITION BY MACHINE_OID, GOAL_OID, GOAL_NAME, GOAL_TYPE ORDER BY %%physloc%%) AS ROW_NUM FROM UG_RESTRICTION) DELETE FROM CTE WHERE ROW_NUM &gt; 1</sql>
        <addPrimaryKey columnNames="MACHINE_OID,GOAL_OID,GOAL_NAME,GOAL_TYPE" constraintName="UG_RESTRICTION_PK" tablespace="MW_IDXENT" tableName="UG_RESTRICTION"/>
        <rollback>
            <dropPrimaryKey constraintName="UG_RESTRICTION_PK" tableName="UG_RESTRICTION"/>
        </rollback>
        <modifySql dbms="mssql,h2,postgresql" applyToRollback="true">
            <regExpReplace replace="( USING INDEX)? (ON|TABLESPACE) MW_[A-Z]+" with=""/>
        </modifySql>
