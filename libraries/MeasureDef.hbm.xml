<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="model" default-lazy="false" default-access="field">

    <class name="minestar.health.api.definition.Measure" table="MEASURE_DEF">

        <meta attribute="noDatabaseMetadata" inherit="false">true</meta>

		<meta attribute="view-create-order">1</meta>
		
        <meta attribute="table">
            tablespace=MW_OBJ
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXOBJ
        </meta>

        <meta attribute="index">
            name=MEASURE_PROTOCOL_DTLS_IDX
            columns=PROTOCOL_ID, PROTOCOL_SUBID ,PROTOCOL_DISPLAYCODE
            tablespace=MW_IDXENT
        </meta>

        <meta attribute="index">
            name=MEASURE_PROTOCOL_IDX
            columns=FAMILY, MEASURE_CATEGORY, PROTOCOL_ID, PROTOCOL_SUBID
            tablespace=MW_IDXENT
        </meta>

        <meta attribute="index">
            name=MEASURE_PROTOCOL_NAME_IDX
            columns=PROTOCOL_NAME
            tablespace=MW_IDXENT
        </meta>

        <meta attribute="viewproperty-description" inherit="false">
            type=string
            description=Measurement description
            expression=lookupResource.measure.description.query
        </meta>

        <meta attribute="viewproperty-universeClassName" inherit="false">
            type=string
            description=This is always null
            expression=lookupResource.null.query
        </meta>

        <meta attribute="viewproperty-unitName" inherit="false">
            type=string
            description=Unit name
            expression=lookupResource.measure.unitName.expr
        </meta>

        <meta attribute="alias-where">
            name=CHANNEL_DEF
            where=MEASURE_DEF.MEASURE_CATEGORY='Channel'
        </meta>

		<!-- The following views are overridden in HealthViews, see comments in HealthReportingViewGenerator.overrideDefViews() for more details. -->
        <meta attribute="alias-where">
            name=DATALOG_DEF
            where=MEASURE_DEF.MEASURE_CATEGORY='Channel'
        </meta>

        <meta attribute="alias-where">
            name=SNAPSHOT_DEF
            where=MEASURE_DEF.MEASURE_CATEGORY='Channel'
        </meta>

        <meta attribute="alias-where">
            name=POLLED_DEF
            where=MEASURE_DEF.MEASURE_CATEGORY='Channel'
        </meta>

        <meta attribute="alias-where">
            name=TREND_DEF
            where=MEASURE_DEF.MEASURE_CATEGORY='Trend'
        </meta>

        <meta attribute="alias-where">
            name=CUM_DEF
            where=MEASURE_DEF.MEASURE_CATEGORY='Cumulative'
        </meta>
        
       <!-- For V_CHANNEL_CONFIG, V_TREND_CONFIG, V_CUMULATIVE_CONFIG, this column will be renamed to oid in the 
       HealthViews phase as BO reports require a human readable string as the oid for those views -->
       <meta attribute="viewproperty-datatypeoid" inherit="false">
            type=string
            description=Object identifier of this measurement
            expression=lookupResource.measure.tempoid.query
        </meta>

        <meta attribute="viewproperty-channeloid" inherit="false">
            type=string
            description=System object identifier of the channel associated with this measurement
            expression=lookupResource.measure.channeloid.query
        </meta>

        <meta attribute="viewproperty-channel_n" inherit="false">
            type=string
            description=Name of the channel associated with this measurement
            expression=lookupResource.measure.channel_n.query
        </meta>

        <meta attribute="viewproperty-channelgroup_n" inherit="false">
            type=string
            description=The channel group the associated channel belongs to (if any)
            expression=lookupResource.measure.channelgroup_n.query
        </meta>

        <meta attribute="viewproperty-channelgroupoid" inherit="false">
            type=string
            description=System object identifier of the channel group the associated channel belongs to (if any)
            expression=lookupResource.measure.channelgroupoid.query
        </meta>

        <meta attribute="viewproperty-condition" inherit="false">
            type=string
            description=Machine condition
            expression=lookupResource.measure.condition.query
        </meta>

        <meta attribute="viewproperty-mid" inherit="false">
            type=string
            description=Module ID
            expression=lookupResource.measure.mid.query
        </meta>

        <cache usage="read-write" region="measureDef"/>

        <id name="oid" type="long">
            <column name="OID" />
            <generator class="assigned" />
        </id>

        <property name="family"
                  type="minestar.health.impl.definition.MeasureFamilyUserType"
                  column="FAMILY"
                  length="12"
                  not-null="true" />

        <property name="category"
                  type="minestar.health.impl.definition.MeasureCategoryUserType"
                  column="MEASURE_CATEGORY"
                  length="16"
                  not-null="true" />

        <property name="method"
                  type="minestar.health.impl.definition.MeasureMethodUserType"
                  column="MEASURE_METHOD"
                  length="16"
                  not-null="false">
            <meta attribute="alias">TYPE</meta>
            <meta attribute="description">The type of trend (avg, min, max) or other type of measure sampled. ie. Summation, Sample.</meta>
        </property>
        
        <property name="source"
                  type="minestar.health.impl.definition.MeasureSourceUserType"
                  length="16">
            <meta attribute="description">The source of this measure definition</meta>
            <column name="MEASURE_SOURCE" default="SYSTEM"/>
        </property>

        <property name="name" type="java.lang.String" column="NAME" length="100" />

        <property name="favorite" type="boolean" column="FAVORITE" />

        <property name="protocolDisplayCode" type="java.lang.Long" column="PROTOCOL_DISPLAYCODE">
            <meta attribute="alias">displaycode</meta>
        </property>

        <property name="configId" type="java.lang.Integer" column="CONFIG_ID" not-null="false" />

        <property name="protocolId" type="long" column="PROTOCOL_ID">
            <meta attribute="alias">id</meta>
        </property>

        <property name="protocolSubId" type="short" column="PROTOCOL_SUBID">
            <meta attribute="alias">subid</meta>
        </property>

        <property name="protocolName" type="java.lang.String" length="20" column="PROTOCOL_NAME" not-null="false">
            <meta attribute="alias">ALTERNATENAME</meta>
        </property>

        <property name="protocolUnitName" type="java.lang.String" length="48" column="PROTOCOL_UNIT_NAME" not-null="true">
        </property>

        <property name="protocolScale" type="double" column="PROTOCOL_SAMPLE_SCALE" not-null="true" />

        <property name="protocolByteSize" type="java.lang.Short" column="PROTOCOL_BYTE_SIZE" not-null="false" />

        <property name="protocolLsb" type="boolean">
            <column name="PROTOCOL_LSB" not-null="true" default="0"/>
        </property>
        
        <property name="protocolOffset" type="double" column="PROTOCOL_OFFSET" />
		
        <property name="protocolMinValid" type="double" column="PROTOCOL_MIN_VALID" />
		
        <property name="protocolMaxValid" type="double" column="PROTOCOL_MAX_VALID" />
		
        <property name="protocolSampleSigned" type="boolean" column="PROTOCOL_SAMPLE_SIGNED" not-null="true" />

        <property name="protocolPid" type="java.lang.Long" column="PROTOCOL_PID" not-null="false" />

        <property name="vmdbVersionSince" type="java.lang.String" length="8" column="VMDB_VERSION_SINCE" />

        <property name="vmdbVersionChanged" type="java.lang.String" length="8" column="VMDB_VERSION_LASTCHANGED" />

        <property name="sampleDelay" type="int" column="SAMPLE_DELAY" not-null="true">
            <meta attribute="alias">SAMPLERATE</meta>
            <meta attribute="description">The intended rate or delay between samples taken.</meta>
        </property>

        <property name="unitType" type="java.lang.String" length="32" column="UNIT_TYPE" not-null="true" />

        <many-to-one name="module" class="minestar.health.api.definition.ModuleDef" column="MODULE_DEF_OID" not-null="false" lazy="false" />

        <many-to-one name="condition" class="minestar.health.api.definition.ConditionDef" column="CONDITION_DEF_OID" not-null="false" lazy="false">
            <meta attribute="alias">conditionDef</meta>
        </many-to-one>

        <property name="valuesContainFMI" type="java.lang.Boolean" column="VALUES_CONTAIN_FMI" access="field"/>

        <property name="valuesAreBoolean" type="java.lang.Boolean" column="VALUES_ARE_BOOLEAN" access="field"/>

    </class>
</hibernate-mapping>
