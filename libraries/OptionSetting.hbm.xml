<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="model">

    <class
            name="minestar.platform.domain.OptionSet"
            table="TBL_OPTION_SETTING">

        <meta attribute="table">
            tablespace=MW_ENT
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXENT
        </meta>

        <meta attribute="index">
            name=OPTION_SETTING_UNIQ
            columns=OPTION_SET, NAME
            tablespace=MW_IDXOBJ
        </meta>
        <meta attribute="description">The Option Set.</meta>

        <id
                name="Oid"
                type="java.lang.Long"
                column="OID"
        >
            <generator class="assigned"/>
        </id>

        <property
                name="optionSet"
                type="java.lang.String"
                column="OPTION_SET"
                not-null="true"
                length="254"
        >
            <meta attribute="description">The Option Set.</meta>
        </property>
        <property
                name="name"
                type="java.lang.String"
                column="NAME"
                not-null="true"
                length="254"
        >
            <meta attribute="description">The Option Set name.</meta>
        </property>
        <property
                name="value"
                type="java.lang.String"
                column="VALUE"
                length="4000"
        >
            <meta attribute="description">The Option Set value.</meta>
        </property>
        <property
                name="defaultValue"
                type="java.lang.String"
                column="DEFAULT_VALUE"
                length="4000"
        >
            <meta attribute="description">The Option Set default value.</meta>
        </property>
    </class>
</hibernate-mapping>
