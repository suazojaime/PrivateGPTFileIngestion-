    <changeSet id="f759782e-87ae-4267-ba93-9699d79cdd2d" author="MineStar" context="Underground/Underground_Management" failOnError="false">
        <preConditions onFailMessage="Ignored AddColumn for MODEL_UPDATE_VERSION to table DRAW_POINT as table does not exist or column already exists." onFail="MARK_RAN">
            <tableExists tableName="DRAW_POINT"/>
            <not>
                <columnExists tableName="DRAW_POINT" columnName="MODEL_UPDATE_VERSION"/>
            </not>
        </preConditions>
        <comment>FUG-3547: AddColumn DRAW_POINT.MODEL_UPDATE_VERSION</comment>
        <addColumn tableName="DRAW_POINT">
            <column name="MODEL_UPDATE_VERSION" type="BIGINT" remarks="The is a copy of the system wide version number."/>
