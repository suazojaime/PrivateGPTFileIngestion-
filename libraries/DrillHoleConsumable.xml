<classdefs>
    <classdef name="minestar.production.domain.cycle.DrillHoleConsumable">
        <propertydef name="drillStart">
            <description>The time drilling started</description>
        </propertydef>
        <propertydef name="drillFinish">
            <description>The time drilling finished</description>
        </propertydef>
        <propertydef name="consumableName">
            <description>The name of the consumable, e.g. its serial number</description>
        </propertydef>
        <propertydef name="consumableType">
            <description>The type of the consumable</description>
        </propertydef>
        <propertydef name="consumableCount">
            <description></description>
        </propertydef>
        <propertydef name="meterage">
            <description>Metres drilled by this consumable during this cycle</description>
        </propertydef>
        <propertydef name="rotations">
            <description>Number of rotations by this consumable during this cycle</description>
        </propertydef>
        <propertydef name="energyDissipated">
            <description>Energy dissipated by the bit in this cycle</description>
        </propertydef>
    </classdef>
</classdefs>
