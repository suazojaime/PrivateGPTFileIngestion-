            <indexExists indexName="SHIFTCHANGE_A_TIEDOWN" tableName="SHIFTCHANGE_A_TIEDOWN"/>
        </preConditions>
        <comment>AHS-7398: DropIndex SHIFTCHANGE_A_TIEDOWN from SHIFTCHANGE_A_TIEDOWN(ECF_CLASS_ID,SHIFTCHANGE_A_TIEDOWN_OID)
        </comment>
        <dropIndex tableName="SHIFTCHANGE_A_TIEDOWN" indexName="SHIFTCHANGE_A_TIEDOWN"/>
        <rollback>
            <createIndex unique="false" tablespace="MW_IDXENT" tableName="SHIFTCHANGE_A_TIEDOWN" indexName="SHIFTCHANGE_A_TIEDOWN">
                <column name="ECF_CLASS_ID"/>
                <column name="SHIFTCHANGE_A_TIEDOWN_OID"/>
            </createIndex>
        </rollback>
        <modifySql dbms="mssql,h2,postgresql">
            <regExpReplace replace="( USING INDEX)? (ON|TABLESPACE) MW_[A-Z]+" with=""/>
        </modifySql>
    </changeSet>
    
