<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="model">

    <class
            name="minestar.external.domain.QrtzFiredTrigger"
            table="QRTZ_FIRED_TRIGGERS"
    >
        <meta attribute="table">
            tablespace=MW_OBJ
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXOBJ
        </meta>
        <meta attribute="description">The Quartz fired triggers</meta>

        <id
                name="entryId"
                type="java.lang.String"
                column="ENTRY_ID"
        >
            <meta attribute="description">The entry ID.</meta>
            <generator class="assigned"/>
        </id>

        <property
                name="triggerName"
                type="java.lang.String"
                column="TRIGGER_NAME"
                not-null="true"
                length="254"
        >
            <meta attribute="description">The name of the trigger</meta>
        </property>
        <property
                name="triggerGroup"
                type="java.lang.String"
                column="TRIGGER_GROUP"
                not-null="true"
                length="254"
        >
            <meta attribute="description">The trigger group</meta>
        </property>
        <property
                name="instanceName"
                type="java.lang.String"
                column="INSTANCE_NAME"
                not-null="true"
                length="254"
        >
            <meta attribute="description">The trigger instance name</meta>
        </property>

        <property
                name="firedTime"
                type="long"
                column="FIRED_TIME"
                not-null="true"
                length="13"
        >
            <meta attribute="description">The fired epoc time</meta>
        </property>
        <property
                name="state"
                type="java.lang.String"
                column="STATE"
                not-null="true"
                length="16"
        >
            <meta attribute="description">The state of the trigger</meta>
        </property>
        <property
                name="jobName"
                type="java.lang.String"
                column="JOB_NAME"
                length="254"
        >
            <meta attribute="description">The trigger job name</meta>
        </property>
        <property
                name="jobGroup"
                type="java.lang.String"
                column="JOB_GROUP"
                length="254"
        >
            <meta attribute="description">The trigger job group name</meta>
        </property>
        <property
                name="isNonconcurrent"
                type="java.lang.String"
                column="IS_NONCONCURRENT"
                length="1"
        >
            <meta attribute="description">Is the trigger is non concurrent</meta>
        </property>
        <property
                name="requestsRecovery"
                type="java.lang.String"
                column="REQUESTS_RECOVERY"
                length="1"
        >
            <meta attribute="description">The trigger recovery requests</meta>
        </property>
        <!-- Data type changed from sql to generic : MSTAR:7000 Nags -->
        <property
                name="priority"
                type="long"
                length="13"
        >
            <meta attribute="defaultValue">5</meta>
            <meta attribute="description">The trigger priority</meta>
            <column name="PRIORITY"/>
        </property>
        <property name="schedName" type="java.lang.String">
            <meta attribute="description">The Scheduler name.</meta>
            <column name="SCHED_NAME" length="120" not-null="true" default="Sched1" />
        </property>
        <property name="schedTime" type="long">
            <meta attribute="description">The Scheduler time.</meta>
            <column name="SCHED_TIME" length="13" not-null="true" default="0" />
        </property>

        <!-- associations -->

    </class>
</hibernate-mapping>
