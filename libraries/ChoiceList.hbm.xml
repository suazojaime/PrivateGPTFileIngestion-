<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="model" default-access="field" default-lazy="false">

    <class name="minestar.platform.domain.ChoiceList" table="TBL_CHOICELIST">

        <meta attribute="table">
            tablespace=MW_ENT
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXENT
        </meta>

        <meta attribute="index">
            name=CHOICELIST_ACTIVE_NUMBER
            columns=CHOICE_LIST, F_ACTIVE, CODE_NUMBER
            tablespace=MW_IDXOBJ
        </meta>
        <meta attribute="index">
            name=CHOICELIST_ACTIVE_STRING
            columns=CHOICE_LIST, F_ACTIVE, CODE_STRING
            tablespace=MW_IDXOBJ
        </meta>

        <meta attribute="description">The Choice List.</meta>

        <id name="oid" type="long" column="OID">
            <generator class="assigned"/>
        </id>

        <property name="choiceList" type="string" column="CHOICE_LIST" length="254" not-null="true">
            <meta attribute="description">The name of the Choice List.</meta>
        </property>

        <property name="codeType" type="string" column="CODE_TYPE" length="1" not-null="true">
            <meta attribute="description">The code type of the Choice List (N=number, C=string).</meta>
        </property>

        <property name="codeString" type="string" column="CODE_STRING" length="254">
            <meta attribute="description">The code string of the Choice List.</meta>
        </property>

        <property name="codeNumber" type="int" column="CODE_NUMBER" not-null="true">
            <meta attribute="description">The code number of the Choice List.</meta>
        </property>

        <property name="description" type="string" column="DESCRIPTION" length="254" not-null="true">
            <meta attribute="description">The description of the Choice List.</meta>
        </property>

        <property name="displayOrder" type="int" column="DISPLAY_ORDER" not-null="true">
            <meta attribute="description">The display order of the Choice List.</meta>
        </property>

        <property name="isActive" type="string" column="F_ACTIVE" length="1" not-null="true">
            <meta attribute="description">Is the Choice List active.</meta>
        </property>

        <property name="inactiveDate" type="timestamp" column="INACTIVE_DATE">
            <meta attribute="description">The Choice List inactive date.</meta>
        </property>

        <property name="inactivator" type="string" column="INACTIVATOR" length="254">
            <meta attribute="description">The Choice List inactivator.</meta>
        </property>

    </class>

</hibernate-mapping>
