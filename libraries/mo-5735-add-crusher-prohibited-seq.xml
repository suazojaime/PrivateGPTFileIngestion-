            <tableExists tableName="MACHINE_IN_PIT"/>
            <not>
                <columnExists tableName="MACHINE_IN_PIT" columnName="PROHIBITED_SEQ_OCCURRENCES"/>
            </not>
        </preConditions>
        <comment>MO-5735: AddColumn MACHINE_IN_PIT.PROHIBITED_SEQ_OCCURRENCES</comment>
        <addColumn tableName="MACHINE_IN_PIT">
            <column name="PROHIBITED_SEQ_OCCURRENCES" type="INT" defaultValueNumeric="0">
