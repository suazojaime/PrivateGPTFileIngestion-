        <comment>MO-2742: CreateTable PROD_PLAN_DUMP_BLEND</comment>
        <createTable tablespace="MW_ENT" remarks="Store Plan Dump Blend details" tableName="PROD_PLAN_DUMP_BLEND">
            <column name="OID" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="BLEND_REFERENCE_ID" type="VARCHAR(255)"/>
            <column name="BLEND_OID" type="BIGINT"/>
