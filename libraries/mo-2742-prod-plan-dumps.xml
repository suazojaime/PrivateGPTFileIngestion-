            <column name="PLAN_OID" type="BIGINT"/>
            <column name="DUMP_BLEND_NR" type="INT"/>
        </createTable>
        <addPrimaryKey columnNames="OID" constraintName="PROD_PLAN_DUMP_BLEND_PK" tablespace="MW_IDXENT" tableName="PROD_PLAN_DUMP_BLEND"/>
