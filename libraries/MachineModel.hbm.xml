<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="minestar.health.api.definition" schema="model">

    <class name="MachineModel" table="MACHINE_MODEL">
        <id name="oid" type="string" length="100" column="OID">
            <generator class="assigned"/>
        </id>
        <property name="serialNumPrefix" type="java.lang.String" column="SERIAL_NUM_PREFIX" length="20"/>
        <property name="modelCode" type="java.lang.String" column="MODEL_CODE" length="20"/>
        <property name="family" type="java.lang.String" column="FAMILY" length="200"/>
        <property name="familyCode" type="java.lang.String" column="FAMILY_CODE" length="2"/>
    </class>
</hibernate-mapping>
