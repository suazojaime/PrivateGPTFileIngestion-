    <classdef name="makeDataStores" >
        <implclass name='{preferredStyle:none}'/>
        <propertydef name="banner" label="">
            <uidef widget="textArea" separator="" custom="{lineWrap = true}" enabledwhen="false" updateable="false"/>
            <defaultvalue class="java.lang.String">WARNING: Supervisor needs to be restarted after running makeDataStores.</defaultvalue>
        </propertydef>
        <propertydef name="Targets" label="Targets">
            <typedef><type code="string" choices='["Checks", "Schema", "ModelData", "Views", "ReferenceData", "ConsistencyCheck", "HealthViews"]' /></typedef>
            <uidef widget="checkbox" separator=" " />
            <defaultvalue class="java.lang.String">Checks Schema ModelData Views ReferenceData ConsistencyCheck HealthViews</defaultvalue>
        </propertydef>
        <propertygroup name="Advanced Options">
	    <uigroupdef mode="collapsed"/>
            <propertygroup name="Schema">
                <propertygroup name="Auto Correction">
                    <propertydef name="autoCorrect" label="Auto-Correct Schema Differences">
                        <typedef>
                            <type code="boolean" />
                        </typedef>
                        <uidef option="-autoCorrect" processOnChange="true"/>
                        <description>Select this option to auto-correct for schema differences that were not handled by change sets.  **** Use this option with caution as it bypasses the usual method of schema change control! ****</description>
                    </propertydef>
                    <propertydef name="autoCorrectScriptOnly" label="Output Script Only">
                        <typedef>
                            <type code="boolean" />
                        </typedef>
                        <uidef option="-scriptOnly" enabledwhen="form.getFieldValue('autoCorrect')==true"/>
                        <description>Select this option to print the change sets used to auto-correct for schema differences that are not handled by existing change sets.  If this option is selected, no schema changes will be made to the database.</description>
                    </propertydef>
                    <propertydef name="removeOrphans" label="Remove Orphaned Data">
                        <typedef>
                            <type code="boolean" />
                        </typedef>
                        <uidef option="-removeOrphans" enabledwhen="form.getFieldValue('autoCorrect')==true"/>
                        <description>Select this option to remove child data that has been orphaned by the removal of its parent.</description>
                    </propertydef>
                    <propertydef name="removeDuplicates" label="Remove Duplicate Data">
                        <typedef>
                            <type code="boolean" />
                        </typedef>
                        <uidef option="-removeDuplicates" enabledwhen="form.getFieldValue('autoCorrect')==true"/>
                        <description>Select this option to remove duplicate data that is preventing a primary key from being added to a table.</description>
                    </propertydef>
                </propertygroup>
                <propertygroup name="Rollback">
                    <propertydef name="rollback" label="Rollback To Release">
                        <typedef>
                            <type code="string" choices="service(minestar.mine.application.MineStarReleaseCollectionProvider):previousReleaseTags"/>
                        </typedef>
                        <uidef option="-rollback {0}" widget="combo" processOnChange="true"/>
                        <description>Select this option to rollback any schema changes to the given release.  Use with caution as this may result in data loss!  Ensure that a snapshotSystem and/or exportDataStores (for at least Model, PitModel and Historical) is created before rollback.  Note that Schema rollback must be performed before rolling back the MineStar installation so that the appropriate rollback instructions are available.</description>
                    </propertydef>
                    <propertydef name="rollbackScriptOnly" label="Output Script Only">
                        <typedef>
                            <type code="boolean" />
                        </typedef>
                        <uidef option="-scriptOnly" enabledwhen="form.getFieldValue('rollback') != '--Select Rollback To Release--'"/>
                        <description>Select this option to print the SQL used to rollback any schema changes to the given release.  If this option is selected, no schema changes will be made to the database.</description>
                    </propertydef>
                </propertygroup>
                <propertydef name="verboseSchema" label="Verbose mode during Schema stage">
                    <typedef>
                        <type code="boolean" />
                    </typedef>
                    <uidef option="-verboseSchema" />
                </propertydef>
            </propertygroup>
            <propertygroup name="Views">
                <propertydef name="riSchema" label="Schema Generation with Relational Integrity into Template">
                    <typedef>
                        <type code="boolean" />
                    </typedef>
                    <uidef option="-riSchema" />
                    <defaultvalue>false</defaultvalue>
                </propertydef>
                <propertydef name="verboseViews" label="Verbose mode during Views stage">
                    <typedef><type code="boolean" /></typedef>
                    <uidef option="-verboseViews" />
                </propertydef>
                <propertydef name="warnWhenShorteningViews" label="Warn when shortening view names">
                    <typedef>
                        <type code="boolean" />
                    </typedef>
                    <uidef option="-warnWhenShorteningViews" />
                </propertydef>
                <propertydef name="skipViews" label="Skip View Generation">
                    <typedef>
                        <type code="boolean" />
                    </typedef>
                    <uidef option="-skipViews" />
                </propertydef>
                <propertydef name="skipConstraints" label="Skip View Constraint Generation">
                    <typedef>
                        <type code="boolean" />
                    </typedef>
                    <uidef option="-skipConstraints" />
                    <defaultvalue>true</defaultvalue>
                </propertydef>
                <propertydef name="skipHistoricalLookups" label="Skip Lookup Generation For Historical Entities">
                    <typedef>
                        <type code="boolean" />
                    </typedef>
                    <uidef option="-skipHistoricalLookups" />
                    <defaultvalue>true</defaultvalue>
                </propertydef>
                <propertydef name="skipUniverse" label="Skip Universe Generation">
                    <typedef>
                        <type code="boolean" />
                    </typedef>
                    <uidef option="-skipUniverse" />
                </propertydef>
                <propertydef name="skipReportingMetadata" label="Skip Reporting Metadata">
                    <typedef>
                        <type code="boolean" />
                    </typedef>
                    <uidef option="-skipReportingMetadata" />
                </propertydef>
                <propertydef name="numericsAsMeasures" label="Generate Numeric Properties As Measures">
                    <typedef>
                        <type code="boolean" />
                    </typedef>
                    <uidef option="-numericsAsMeasures" />
                </propertydef>
            </propertygroup>
            <propertygroup name="ConsistencyCheck">
                <propertydef name="checkHistorical" label="Check Historical Database">
                    <typedef>
                        <type code="boolean" />
                    </typedef>
		            <uidef option="-checkHistorical" />
		            <description>Select this option only when the historical database should be checked.</description>
                </propertydef>
                <propertydef name="commit" label="Commit Changes">
                    <typedef>
                        <type code="boolean" />
                    </typedef>
		            <uidef option="-commit" />
		            <description>Select this option to commit any required database changes.</description>
                </propertydef>
            </propertygroup>
        </propertygroup>
    </classdef>

