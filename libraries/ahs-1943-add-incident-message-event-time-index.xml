<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="42a66fcc-0b47-44eb-be90-9085c183dc24" author="MineStar" failOnError="false">
        <preConditions onFailMessage="Ignored CreateIndex for INCIDENT_MESSAGE_EVENT_TIME to table INCIDENT_MESSAGE as (1) table does not exist, or (2) index already exists." onFail="MARK_RAN">
            <tableExists tableName="INCIDENT_MESSAGE"/>
            <not>
                <indexExists indexName="INCIDENT_MESSAGE_EVENT_TIME" tableName="INCIDENT_MESSAGE"/>
            </not>
        </preConditions>
        <comment>AHS-1943: CreateIndex INCIDENT_MESSAGE_EVENT_TIME on INCIDENT_MESSAGE (EVENT_TIMESTAMP_UTC)</comment>
        <createIndex unique="false" tablespace="MW_IDXENT" tableName="INCIDENT_MESSAGE" indexName="INCIDENT_MESSAGE_EVENT_TIME">
            <column name="EVENT_TIMESTAMP_UTC"/>
        </createIndex>
        <modifySql dbms="mssql,h2,postgresql">
            <regExpReplace replace="( USING INDEX)? (ON|TABLESPACE) MW_[A-Z]+" with=""/>
        </modifySql>
    </changeSet>

</databaseChangeLog>
