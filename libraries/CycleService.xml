<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
          http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
      ">

    <bean id="cycleService" class="minestar.production.service.cycle.CycleService" factory-method="getInstance" lazy-init="true"/>
    <bean id="materialTrackingService" class="minestar.production.service.material.MaterialTrackingService" factory-method="getInstance" lazy-init="true"/>
    <bean id="materialManagementService" class="minestar.mine.service.material.MaterialManagementService" factory-method="getInstance" lazy-init="true"/>
    <bean id="mineFleetService" class="minestar.pitlink.service.fleet.MineFleetService" factory-method="getInstance" lazy-init="true"/>
    <bean id="timelineService" class="minestar.production.service.timeline.TimelineService" factory-method="getInstance" lazy-init="true"/>

    <bean id="cycleSynchronizerService" class="minestar.production.service.cycle.CycleSynchronizerService" factory-method="getInstance" lazy-init="true"/>

</beans>
