<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="minestar.health.domain.metadata" schema="model" default-access="field" default-lazy="false">

    <class name="ChannelDescription" table="CHANNEL_DISPLAY">
        <meta attribute="noDatabaseMetadata" inherit="false">true</meta>
        <meta attribute="lookupExpr" inherit="false">description</meta>
        <meta attribute="table">
            tablespace=MW_OBJ
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXOBJ
        </meta>

        <meta attribute="index">
            name=CHANNEL_DISPLAY_IDX
            columns=CHANNELVALUE ,DISPLAYCODE
            tablespace=MW_IDXENT
        </meta>

        <id name="oid" type="java.lang.String" column="OID">
            <generator class="assigned"/>
        </id>

        <property name="id" column="ID" not-null="true"/>
        <property name="displayCode" column="DISPLAYCODE" not-null="true"/>
        <property name="description" column="DESCRIPTION" length="254" not-null="true"/>
        <property name="channelValue" column="CHANNELVALUE" not-null="true"/>
        <property name="altValue" column="ALTVALUE"/>

    </class>
</hibernate-mapping>
