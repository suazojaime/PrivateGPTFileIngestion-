<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:widget="http://www.minestar.au.cat.com/namespace/widget"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.minestar.au.cat.com/namespace/widget http://www.minestar.au.cat.com/namespace/widget.xsd
       ">

    <widget:guiGroup id="drawCardComplianceGroup" title="Draw Card Compliance" orientation="vertical">
        <widget:componentRef ref="drawCardComplianceCharts"/>
        <!-- widget:componentRef ref="debugDrawCardComplianceTables"/ -->
    </widget:guiGroup>

    <widget:guiGroup id="drawCardComplianceCharts" groupMode="plain" orientation="horizontal">
        <widget:componentRef ref="totalDrawCardComplianceChart"/>
        <widget:componentRef ref="panelDrawCardComplianceChart"/>
    </widget:guiGroup>

    <widget:guiGroup id="debugDrawCardComplianceTables" groupMode="plain" orientation="horizontal">
        <widget:componentRef ref="drawPointDrawCardComplianceTable"/>
    </widget:guiGroup>

</beans>
