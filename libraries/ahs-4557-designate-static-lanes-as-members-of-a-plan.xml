    <changeSet id="7af22518-60f8-4691-b65d-27ec66a13c52" author="MineStar" context="Machine_Tracking/Machine_Tracking_Management" failOnError="false">
        <preConditions onFailMessage="Ignored AddForeignKeyConstraint for PLAN_LANE_INFO_INBOUND_ACC_FK1 to table PLAN_LANE_INFO_INBOUND_ACCESS as (1) the table does not exist, or (2) the foreignKey already exists." onFail="MARK_RAN">
            <tableExists tableName="PLAN_LANE_INFO_INBOUND_ACCESS"/>
            <tableExists tableName="LANE"/>
            <not>
                <foreignKeyConstraintExists foreignKeyTableName="PLAN_LANE_INFO_INBOUND_ACCESS" foreignKeyName="PLAN_LANE_INFO_INBOUND_ACC_FK1"/>
            </not>
        </preConditions>
        <comment>AHS-4557: AddForeignKeyConstraint PLAN_LANE_INFO_INBOUND_ACC_FK1 to PLAN_LANE_INFO_INBOUND_ACCESS (LANE_OID)</comment>
        <sql>DELETE FROM PLAN_LANE_INFO_INBOUND_ACCESS WHERE LANE_OID IS NOT NULL AND NOT EXISTS (SELECT 'x' FROM LANE WHERE LANE.LANE_OID = PLAN_LANE_INFO_INBOUND_ACCESS.LANE_OID)</sql>
        <addForeignKeyConstraint baseTableName="PLAN_LANE_INFO_INBOUND_ACCESS" baseColumnNames="LANE_OID" constraintName="PLAN_LANE_INFO_INBOUND_ACC_FK1" referencedTableName="LANE" referencedColumnNames="LANE_OID" onDelete="NO ACTION"/>
        <rollback>
            <dropForeignKeyConstraint baseTableName="PLAN_LANE_INFO_INBOUND_ACCESS" constraintName="PLAN_LANE_INFO_INBOUND_ACC_FK1"/>
        </rollback>
    </changeSet>
