    <changeSet id="3511d66d-6829-442a-a779-b341b07d695a" author="MineStar" context="Machine_Tracking/Machine_Tracking_Management" failOnError="false">
        <preConditions onFailMessage="Ignored CreateTable for PLAN_LANE_INFO_INBOUND_ACCESS as table already exists." onFail="MARK_RAN">
            <not>
                <tableExists tableName="PLAN_LANE_INFO_INBOUND_ACCESS"/>
            </not>
        </preConditions>
        <comment>AHS-4557: CreateTable PLAN_LANE_INFO_INBOUND_ACCESS</comment>
        <createTable tablespace="MW_ENT" remarks="The Lanes that connect this plan's entry lanes to the mine's haulage lanes." tableName="PLAN_LANE_INFO_INBOUND_ACCESS">
            <column name="PLAN_OID" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="LANE_OID" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <addPrimaryKey columnNames="PLAN_OID,LANE_OID" constraintName="PLAN_LANE_INFO_INBOUND_ACCE_PK" tablespace="MW_IDXENT" tableName="PLAN_LANE_INFO_INBOUND_ACCESS"/>
        <modifySql dbms="mssql,h2,postgresql">
            <regExpReplace replace="( USING INDEX)? (ON|TABLESPACE) MW_[A-Z]+" with=""/>
        </modifySql>
    </changeSet>
