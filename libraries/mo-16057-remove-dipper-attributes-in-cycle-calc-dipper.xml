<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="cf5c90e2-db81-4a33-8b7e-7402c50c8540" author="MineStar" failOnError="false">
        <preConditions onFailMessage="Ignored DropColumn for DIPPER_ATTRIBUTES of table CYCLE_CALC_DIPPER as column does not exist."
                       onFail="MARK_RAN">
            <columnExists tableName="CYCLE_CALC_DIPPER" columnName="DIPPER_ATTRIBUTES"/>
        </preConditions>
        <comment>MO-16057: DropColumn CYCLE_CALC_DIPPER.DIPPER_ATTRIBUTES</comment>
        <dropColumn columnName="DIPPER_ATTRIBUTES" tableName="CYCLE_CALC_DIPPER"/>
        <rollback>
            <addColumn tableName="CYCLE_CALC_DIPPER">
                <column name="DIPPER_ATTRIBUTES" type="NCLOB" remarks="The dipper attributes of the cycle."/>
            </addColumn>
        </rollback>
    </changeSet>
</databaseChangeLog>
