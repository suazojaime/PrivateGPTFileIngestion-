<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
  		http://www.springframework.org/schema/beans/spring-beans.xsd"
        >

  <!-- Dependencies go here  any id's should be prefixed with your extension short name -->
  <!-- 
  Sample Dependencie
  <bean id="cycleUCC" parent="parentRemote">
    <property name="serviceUrl" value="rmi://${MSTAR_BUS_HOST}:${MSTAR_BUS_PORT}/cycleUCC"/>
    <property name="serviceInterface" value="minestar.production.uifacade.cycle.CycleUCCFacade"/>
  </bean> 
  -->

  <!-- Exposed api interfaces go here and dependencies should be included -->
  <!--
  Sample Interface 
  <bean class="minestar.api.production.cycle.Cycle">
    <property name="cycleUCC" ref="cycleUCC"/>
  </bean>
  -->

 </beans>
