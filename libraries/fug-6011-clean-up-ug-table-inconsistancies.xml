    <changeSet id="703a4aaa-c2ba-4b98-a1e4-dbbe8dc5825e" author="MineStar" context="Underground/Underground_Management" failOnError="false">
        <preConditions onFailMessage="Ignored DropIndex for IDX_ADC_CREATED_DATE of table ACTIVE_DRAW_CARD as index does not exist." onFail="MARK_RAN">
            <indexExists indexName="IDX_ADC_CREATED_DATE" tableName="ACTIVE_DRAW_CARD"/>
        </preConditions>
        <comment>FUG-6011: DropIndex IDX_ADC_CREATED_DATE from ACTIVE_DRAW_CARD (CREATED_DATE_UTC)</comment>
        <dropIndex tableName="ACTIVE_DRAW_CARD" indexName="IDX_ADC_CREATED_DATE"/>
        <rollback>
            <createIndex unique="false" tableName="ACTIVE_DRAW_CARD" indexName="IDX_ADC_CREATED_DATE">
                <column name="CREATED_DATE_UTC"/>
            </createIndex>
        </rollback>
        <modifySql dbms="mssql,h2,postgresql">
            <regExpReplace replace="( USING INDEX)? (ON|TABLESPACE) MW_[A-Z]+" with=""/>
        </modifySql>
    </changeSet>
