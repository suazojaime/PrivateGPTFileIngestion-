<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:widget="http://www.minestar.au.cat.com/namespace/widget"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.minestar.au.cat.com/namespace/widget http://www.minestar.au.cat.com/namespace/widget.xsd
       ">

    <widget:legend id="customTruckCycleActivityLegend">
        <widget:series colour="travellingEmptyColor" index="0"/>
        <widget:series colour="loadingTimeColor" index="1"/>
        <widget:series colour="travellingFullColor" index="2"/>
        <widget:series colour="dumpingTimeColor" index="3"/>
    </widget:legend>

    <widget:legend id="customLhdCycleActivityLegend">
        <widget:series colour="travellingEmptyColor" index="0"/>
        <widget:series colour="loadingTimeColor" index="1"/>
        <widget:series colour="travellingFullColor" index="2"/>
        <widget:series colour="dumpingTimeColor" index="3"/>
    </widget:legend>

    <widget:legend id="customTruckActivityBreakdownLegend">
        <widget:series colour="travellingEmptyColor" index="travellingEmptyTime"/>
        <widget:series colour="loadingTimeColor" index="loadingTime"/>
        <widget:series colour="travellingFullColor" index="travellingFullTime"/>
        <widget:series colour="dumpingTimeColor" index="dumpingTime"/>
    </widget:legend>

    <widget:legend id="customLhdActivityBreakdownLegend">
        <widget:series colour="travellingEmptyColor" index="travellingEmptyTime"/>
        <widget:series colour="loadingTimeColor" index="loadingTime"/>
        <widget:series colour="travellingFullColor" index="travellingFullTime"/>
        <widget:series colour="dumpingTimeColor" index="dumpingTime"/>
    </widget:legend>

    <widget:legend id="customDrawCardComplianceLegend">
        <widget:series colour="underPullColor" index="0"/>
        <widget:series colour="complyColor" index="1"/>
        <widget:series colour="overPullColor" index="2"/>
    </widget:legend>

</beans>
