<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       ">

    <bean id="mineFuelService" class="minestar.production.service.fuel.MineFuelService" factory-method="getInstance" lazy-init="true"/>

    <bean id="mineFleetService" class="minestar.pitlink.service.fleet.MineFleetService" factory-method="getInstance" lazy-init="true"/>

    <bean id="machineRestrictionService" class="minestar.pitlink.service.fleet.MachineRestrictionService" factory-method="getInstance" lazy-init="true"/>

    <bean id="sensorService" class="minestar.machinetracking.service.sensor.SensorService" factory-method="getInstance" lazy-init="true"/>

    <bean id="truckSnapshotService" class="minestar.pitlink.service.trucksnapshot.TruckSnapshotService" factory-method="getInstance" lazy-init="true"/>

    <bean id="fleetProductionService" class="minestar.production.service.fleet.FleetProductionService" factory-method="getInstance" lazy-init="true"/>

    <bean id="delayManagementService" class="minestar.production.service.delay.DelayManagementService" factory-method="getInstance" lazy-init="true"/>

    <bean id="mineMessageService" class="minestar.pitlink.service.message.MineMessageService" factory-method="getInstance" lazy-init="true"/>

    <bean id="dimensionManagementService" class="minestar.pitlink.service.fleet.DimensionManagementService" factory-method="getInstance" lazy-init="true"/>

    <bean id="machineDiagnosticsService" class="minestar.pitlink.service.machinediagnostics.MachineDiagnosticsService" factory-method="getInstance" lazy-init="true"/>

    <bean id="fleetAssignmentService" class="minestar.pitlink.service.fleet.assignment.FleetAssignmentService" factory-method="getInstance" lazy-init="true"/>

    <bean id="cableBridgeService" class="minestar.mine.service.minemodel.CableBridgeService" factory-method="getInstance" lazy-init="true"/>

    <bean id="machinePayloadService" class="minestar.pitlink.service.fleet.MachinePayloadService" factory-method="getInstance" lazy-init="true"/>

    <bean id="shiftService" class="minestar.platform.service.calendar.ShiftService" factory-method="getInstance" lazy-init="true"/>

    <bean id="positionService" class="com.mincom.works.positions.service.PositionService" factory-method="getInstance" lazy-init="true"/>

    <bean id="timelineService" class="minestar.production.service.timeline.TimelineService" factory-method="getInstance" lazy-init="true"/>

    <bean id="dipperTimelineService" class="minestar.production.service.timeline.dipper.DipperTimelineService" factory-method="getInstance" lazy-init="true"/>

    <bean id="payloadTimelineService" class="minestar.production.service.timeline.payload.PayloadTimelineService" factory-method="getInstance" lazy-init="true"/>

</beans>
