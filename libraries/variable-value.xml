<?xml version="1.0" encoding="utf-8"?>

<variables xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="..\Schemas\ConsoleScriptVariablesSchema.xsd">

    <variable-group name="ProjectSpecs">
        <variable name="$WorkingFolder$"  value="."/>
        <variable name="$project_name$" value="OracleToSqlServerImporter"/>
        <variable name="$project_overwrite$" value="true"/>
        <variable name="$project_type$" value="sql-server-2008"/>
        <variable name="$project_folder$" value="$WorkingFolder$\$project_name$"/>
    </variable-group>

    <variable-group name="OracleConnection">
        <variable name="$OracleHostName$"  value=""/>
        <variable name="$OracleInstance$"  value="MINESTAR"/>
        <variable name="$OraclePort$"  value="1521"/>
        <variable name="$OracleModelUserName$"  value="posttrunkmodel"/>
        <variable name="$OracleTimeSeriesUserName$"  value="posttimeseries"/>
        <variable name="$OraclePitModelUserName$"  value="posttrunkpitmodel"/>
		<variable name="$OracleHistoricalUserName$"     value="posttrunkhistorical"/>
		<variable name="$OracleSummaryUserName$"     value="posttrunksummary"/>
    </variable-group>

    <variable-group name="SQLServerConnection">
        <variable name="$SQLServerName$"   value="DGC001158983\MINESTAR"/>
        <variable name="$SQLServerModelUserName$"     value="winsqlmodel"/>
        <variable name="$SQLServerTimeSeriesUserName$"   value="winsqltimeseries"/>
        <variable name="$SQLServerPitModelUserName$"     value="winsqlpitmodel"/>
		<variable name="$SQLServerHistoricalUserName$"     value="winsqlhistorical"/>
		<variable name="$SQLServerSummaryUserName$"     value="winsqlsummary"/>
		
    </variable-group>

    <variable-group name="Report">
        <variable name="$SummaryReports$"               value="$WorkingFolder$"/>
        <variable name="$AssessmentReportFolderName$"   value="AssessmentReports"/>
        <variable name="$AssessmentReports$"            value="$WorkingFolder$\$AssessmentReportFolderName$"/>
        <variable name="$RefreshDBFolder$"              value="$WorkingFolder$"/>
        <variable name="$ConvertARReportsFolder$"       value="$WorkingFolder$\$AssessmentReportFolderName$"/>
        <variable name="$SynchronizationReports$"       value="$WorkingFolder$"/>
        <variable name="$SaveScriptFolder$"             value="$WorkingFolder$"/>
        <variable name="$ConvertSQLReports$"            value="$WorkingFolder$"/>
        <variable name="$ConvertSqlSampleFiles$"        value="$OutputFolder$\{Folder where sql files are stored}"/>
    </variable-group>

</variables>


