<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        ">
          
    <bean id="userModeActionServiceImpl" 
          class="minestar.autonomy.service.mode.core.DelegatingUserModeActionService"/>
    
    <bean id="registerUserModeActionService" parent="registrationInstance" lazy-init="true">
        <constructor-arg value="User Mode Action Service"/>
        <constructor-arg value="minestar.autonomy.service.mode.UserModeActionService"/>
        <constructor-arg ref="userModeActionServiceImpl"/>
    </bean>
    
</beans>
