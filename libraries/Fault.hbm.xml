<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="minestar.health.api.definition" schema="model">

    <class name="FaultDef" table="FAULT_DEF">
        <meta attribute="noDatabaseMetadata" inherit="false">true</meta>
        <meta attribute="lookupExpr" inherit="false">fault</meta>
        <meta attribute="table">
            tablespace=MW_OBJ
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXOBJ
        </meta>

        <meta attribute="viewproperty-universeClassName" inherit="false">
            type=string
            expression=lookupResource.null.query
        </meta>
        <meta attribute="viewproperty-family" inherit="false">
            type=string
            expression=lookupResource.null.query
        </meta>
        <meta attribute="viewproperty-id" inherit="false">
            type=string
            expression=lookupResource.fault.id.query
        </meta>


        <id name="oid" type="long" column="OID">
            <generator class="assigned"/>
        </id>

        <property name="sequence" type="int">
            <meta attribute="alias">SequenceNumber</meta>
        </property>

        <many-to-one name="fmi" class="FmiDef"/>
        <many-to-one name="component" class="ComponentDef">
            <meta attribute="alias">cid</meta>
        </many-to-one>

        <property name="fault" length="255"/>

    </class>
</hibernate-mapping>
