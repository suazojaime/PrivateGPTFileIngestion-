        </preConditions>
        <comment>FUG-3833: AddColumn UG_AIS_BARRIER.MODEL_UPDATE_VERSION</comment>
        <addColumn tableName="UG_AIS_BARRIER">
            <column name="MODEL_UPDATE_VERSION" type="BIGINT" remarks="The is a copy of the system wide version number."/>
        </addColumn>
    </changeSet>
</databaseChangeLog>
