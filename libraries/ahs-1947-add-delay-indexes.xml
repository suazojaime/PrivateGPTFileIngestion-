            <indexExists indexName="DELAY_FBIDX_2" tableName="DELAY"/>
        </preConditions>
        <comment>AHS-1947: DropIndex DELAY_FBIDX_2 from DELAY (STATUS,DELAYDISCRIMINATOR(TARGET_MACHINE,TARGET_ROAD,TARGET_LOCATION),START1_UTC,FINISH_UTC)</comment>
        <dropIndex indexName="DELAY_FBIDX_2" tableName="DELAY"/>
        <rollback>
            <createIndex unique="false" tableName="DELAY" indexName="DELAY_FBIDX_2">
                <column name="STATUS"/>
                <column name="DELAYDISCRIMINATOR(TARGET_MACHINE,TARGET_ROAD,TARGET_LOCATION)"/>
                <column name="START1_UTC"/>
                <column name="FINISH_UTC"/>
            </createIndex>
        </rollback>
    </changeSet>

    <changeSet id="c9b5c16c-4ad7-48f6-b141-8e655514643e" author="MineStar" failOnError="false">
        <preConditions onFailMessage="Ignored CreateIndex for DELAY_TARGET_MACHINE to table DELAY as (1) table does not exist, or (2) index already exists." onFail="MARK_RAN">
            <tableExists tableName="DELAY"/>
            <not>
                <indexExists indexName="DELAY_TARGET_MACHINE" tableName="DELAY"/>
            </not>
        </preConditions>
        <comment>AHS-1947: CreateIndex DELAY_TARGET_MACHINE on DELAY (TARGET_MACHINE,STATUS,START1_UTC)</comment>
        <createIndex unique="false" tablespace="MW_IDXENT" tableName="DELAY" indexName="DELAY_TARGET_MACHINE">
            <column name="TARGET_MACHINE"/>
            <column name="STATUS"/>
            <column name="START1_UTC"/>
        </createIndex>
        <modifySql dbms="mssql,h2,postgresql">
            <regExpReplace replace="( USING INDEX)? (ON|TABLESPACE) MW_[A-Z]+" with=""/>
        </modifySql>
    </changeSet>

