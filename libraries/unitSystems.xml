<?xml version="1.0" standalone="no"?>
<unitSystems xmlns="http://www.minestar.cat.com/namespace/unitSystems"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.minestar.cat.com/namespace/unitSystems /unitSystems.xsd">

    <!--****************************************************************** -->
    <!--Definition of unit sets and the default display unit for each type -->
    <!--****************************************************************** -->
    <unitSystem name="Mining SI">
        <preferredUnit quantityType="absorbed dose" unit="gray"/>
        <preferredUnit quantityType="acceleration" unit="metres per second squared"/>
        <preferredUnit quantityType="acidity" unit="mg koh per g"/>
        <preferredUnit quantityType="angle" unit="degrees"/>
        <preferredUnit quantityType="angular speed" unit="revolutions per minute"/>
        <preferredUnit quantityType="angular acceleration" unit="rpm per second"/>
        <preferredUnit quantityType="apparent power" unit="volt ampere"/>
        <preferredUnit quantityType="area" unit="square metres"/>
        <preferredUnit quantityType="bank volume" unit="bank cubic metres"/>
        <preferredUnit quantityType="bank volume flow rate" unit="bank cubic metres per hour"/>
        <preferredUnit quantityType="count per time" unit="count per hour"/>
        <preferredUnit quantityType="curvature" unit="radians per metre"/>
        <preferredUnit quantityType="density" unit="tonnes per cubic metre"/>
        <!--<preferredUnit quantityType="drill speed" unit="metres per minute"/>-->
        <preferredUnit quantityType="electric capacitance" unit="farad"/>
        <preferredUnit quantityType="electric charge" unit="coulomb"/>
        <preferredUnit quantityType="electric conductance" unit="siemens"/>
        <preferredUnit quantityType="electric current" unit="ampere"/>
        <preferredUnit quantityType="electric inductance" unit="henry"/>
        <preferredUnit quantityType="electric potential" unit="volt"/>
        <preferredUnit quantityType="electric resistance" unit="ohm"/>
        <!--<preferredUnit quantityType="electromotive force" unit="volt"/>-->
        <preferredUnit quantityType="energy" unit="joule"/>
        <preferredUnit quantityType="energy per fuel burn" unit="kilowatt hour per litre"/>
        <preferredUnit quantityType="force" unit="newton"/>
        <!--<preferredUnit quantityType="frequency" unit="hertz"/>-->
        <preferredUnit quantityType="fuel burn per distance" unit="litres per kilometre"/>
        <!--<preferredUnit quantityType="fuel burn per time" unit="litres per hour"/>-->
        <preferredUnit quantityType="illuminance" unit="lux"/>
        <preferredUnit quantityType="index" unit="index"/>
        <preferredUnit quantityType="length" unit="metre"/>
        <preferredUnit quantityType="liquid volume" unit="litre"/>
        <preferredUnit quantityType="load" unit="loads"/>
        <preferredUnit quantityType="load rate" unit="loads per hour"/>
        <preferredUnit quantityType="loose volume" unit="loose cubic metres"/>
        <preferredUnit quantityType="loose volume flow rate" unit="loose cubic metres per hour"/>
        <preferredUnit quantityType="luminous flux" unit="lumen"/>
        <preferredUnit quantityType="magnetic flux" unit="weber"/>
        <preferredUnit quantityType="magnetic flux density" unit="tesla"/>
        <preferredUnit quantityType="mass" unit="tonne"/>
        <preferredUnit quantityType="mass distance" unit="tonne kilometre"/>
        <preferredUnit quantityType="mass flow rate" unit="tonnes per hour"/>
        <!--<preferredUnit quantityType="mass ounce" unit="ounce"/>-->
        <preferredUnit quantityType="mass speed" unit="tonne kilometre per hour"/>
        <!--<preferredUnit quantityType="odometer" unit="kilometre"/>-->
        <preferredUnit quantityType="parts per parts" unit="parts per million"/>
        <!--<preferredUnit quantityType="potential difference" unit="volt"/>-->
        <preferredUnit quantityType="power" unit="watt"/>
        <!--<preferredUnit quantityType="precise time" unit="millisecond"/>-->
        <preferredUnit quantityType="pressure" unit="kilopascal"/>
        <preferredUnit quantityType="quantity" unit="count"/>
        <preferredUnit quantityType="radioactivity" unit="becquerel"/>
        <preferredUnit quantityType="ratio" unit="percent"/>
        <preferredUnit quantityType="reactive power" unit="volt ampere reactive"/>
        <!--<preferredUnit quantityType="service meter hours" unit="hour"/>-->
        <preferredUnit quantityType="solid angle" unit="steradian"/>
        <preferredUnit quantityType="speed" unit="kilometres per hour"/>
        <preferredUnit quantityType="storage" unit="byte"/>
        <preferredUnit quantityType="temperature" unit="celsius"/>
        <preferredUnit quantityType="time" unit="second"/>
        <preferredUnit quantityType="torque" unit="newton metre"/>
        <!--<preferredUnit quantityType="unitless" unit="one"/>-->
        <preferredUnit quantityType="viscosity" unit="pascal second"/>
        <preferredUnit quantityType="volume" unit="cubic metres"/>
        <preferredUnit quantityType="volumetric flow rate" unit="litres per hour"/>
        <!--<preferredUnit quantityType="weight on bit" unit="kilonewton"/>-->
        <!--<preferredUnit quantityType="work" unit="joule"/>-->
    </unitSystem>

    <unitSystem name="Mining Imperial" parent="Mining SI">
        <preferredUnit quantityType="area" unit="square feet"/>
        <preferredUnit quantityType="bank volume" unit="bank cubic yards"/>
        <preferredUnit quantityType="bank volume flow rate" unit="bank cubic yards per hour"/>
        <preferredUnit quantityType="curvature" unit="radians per foot"/>
        <preferredUnit quantityType="density" unit="tons per cubic yard"/>
        <!--<preferredUnit quantityType="drill speed" unit="feet per minute"/>-->
        <preferredUnit quantityType="energy" unit="joule"/>
        <preferredUnit quantityType="energy per fuel burn" unit="horsepower hour per gallon"/>
        <preferredUnit quantityType="fuel burn per distance" unit="gallons per mile"/>
        <!--<preferredUnit quantityType="fuel burn per time" unit="gallons per hour"/>-->
        <preferredUnit quantityType="length" unit="foot"/>
        <preferredUnit quantityType="liquid volume" unit="gallon"/>
        <preferredUnit quantityType="loose volume" unit="loose cubic yards"/>
        <preferredUnit quantityType="loose volume flow rate" unit="loose cubic yards per hour"/>
        <preferredUnit quantityType="mass" unit="ton"/>
        <preferredUnit quantityType="mass distance" unit="ton mile"/>
        <preferredUnit quantityType="mass flow rate" unit="tons per hour"/>
        <preferredUnit quantityType="mass speed" unit="ton mile per hour"/>
        <!--<preferredUnit quantityType="odometer" unit="mile"/>-->
        <preferredUnit quantityType="pressure" unit="pounds per square inch"/>
        <preferredUnit quantityType="quantity" unit="count"/>
        <preferredUnit quantityType="speed" unit="miles per hour"/>
        <preferredUnit quantityType="temperature" unit="fahrenheit"/>
        <preferredUnit quantityType="torque" unit="foot pounds"/>
        <preferredUnit quantityType="volume" unit="cubic yards"/>
        <preferredUnit quantityType="volumetric flow rate" unit="gallons per hour"/>
    </unitSystem>

</unitSystems>
