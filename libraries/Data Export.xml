<?xml version="1.0" encoding="us-ascii" standalone="no"?>
<!DOCTYPE classdefs SYSTEM "metadata.dtd">
<classdefs>

    <classdef name="SingleModel" >
        <propertygroup name="Properties" >
            <uigroupdef mode="region" />
            <propertydef name="Custom Fields" label="">
                <typedef><type code="string" choices="metadata:entity($selectedsingle):attributes" /></typedef>
				<uidef widget="ordered" separator=","/>
            </propertydef>
        </propertygroup>
    </classdef>

    <classdef name="BulkModel" >
    </classdef>

    <classdef name="SingleHistorical" >
        <propertygroup name="Properties" >
            <uigroupdef mode="region" />
            <propertydef name="Custom Fields" label ="">
                <typedef><type code="string" choices="metadata:entity($selectedsingle):attributes" /></typedef>
				<uidef widget="ordered" separator=","/>
            </propertydef>
        </propertygroup>
        <propertygroup name="When" >
            <uigroupdef mode="region" />
            <propertydef name="Period" >
                <typedef><type code="string" /></typedef>
				<uidef widget="period" option="Start_Date,End_Date" />
            </propertydef>
            <propertydef name="Start_Date" >
                <typedef><javaclass name="java.util.Date" /></typedef>
            </propertydef>
            <propertydef name="End_Date" >
                <typedef><javaclass name="java.util.Date" /></typedef>
            </propertydef>
            <propertydef name="Time Filter Property" >
                <typedef><type code="string" choices='metadata:entity($selectedsingle):timeAttributes'/></typedef>
				<uidef widget="combo" mode="entry" />
                <description>Property to query against.</description>
            </propertydef>
        </propertygroup>
    </classdef>

    <classdef name="BulkHistorical" >
        <propertygroup name="When" >
            <uigroupdef mode="region" />
            <propertydef name="Period" >
                <typedef><type code="string" /></typedef>
				<uidef widget="period" option="Start_Date,End_Date" />
            </propertydef>
            <propertydef name="Start_Date" >
                <typedef><javaclass name="java.util.Date" /></typedef>
            </propertydef>
            <propertydef name="End_Date" >
                <typedef><javaclass name="java.util.Date" /></typedef>
            </propertydef>
            <propertydef name="Time Filter Property" >
                <typedef><type code="string" choices='metadata:entity($selectedsingle):timeAttributes'/></typedef>
				<uidef widget="combo" mode="entry" />
                <description>Search list (comma-separated) for property to query against.</description>
            </propertydef>
        </propertygroup>
    </classdef>

    <classdef name="Output" >
        <propertygroup name="Filtering" >
            <uigroupdef mode="collapsed" />
            <propertydef name="Filter Expression" >
                <typedef><type code="string" choices='metadata:entity($selectedsingle):attributes'/></typedef>
				<uidef widget="expression"/>
            </propertydef>
        </propertygroup>

        <propertygroup name="Formatting" >
            <uigroupdef mode="collapsed" />
            <propertydef name="Formats" >
                <typedef>
                  <type code="string" choices='{xls="XLS - Microsoft Excel",csv="CSV - Comma Separated Value",xml="XML - Extensible Markup Language",html="HTML - HyperText Markup Language",pdf="PDF - Adobe Acrobat"}' /></typedef>
				  <uidef widget="listbox" separator="," />
                <defaultvalue class="java.lang.String">xls,csv</defaultvalue>
            </propertydef>
            <propertydef name="Orientation" >
                <typedef>
                  <type code="string" choices='{"portrait_export":"Portrait", "landscape_export":"Landscape"}' />
                </typedef>
                <defaultvalue class="java.lang.String">Landscape</defaultvalue>
                <description>Only applicable for PDF export and on screen previewing.</description>
            </propertydef>
        </propertygroup>

        <propertygroup name="Files" >
            <uigroupdef mode="collapsed" />
            <propertydef name="Directory" >
                <typedef><type code="string" /></typedef>
				<uidef widget="file" mask="directory" />
                <defaultvalue type="python">interpretPath('{MSTAR_DATA}')</defaultvalue>
            </propertydef>
            <propertydef name="Filename Pattern">
                <typedef><type code="string" /></typedef>
                <defaultvalue class="java.lang.String">{1}_{2}</defaultvalue>
                <description>Filename excluding extension. Codes supported are: {0}=Group, {1}=Collection, {2}=Date, {3}=Time.</description>
            </propertydef>
        </propertygroup>
    </classdef>

</classdefs>
