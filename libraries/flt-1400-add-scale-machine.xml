    <changeSet id="5f646acb-e187-4108-9483-ce85094b19e8" author="MineStar" context="Pit_Link/Pit_Link_Management" failOnError="true">
        <preConditions onFailMessage="Ignored CreateTable for MACHINE_CLASS_MEASURE_TIME as table already exists." onFail="MARK_RAN">
            <not>
                <tableExists tableName="MACHINE_CLASS_MEASURE_TIME"/>
            </not>
        </preConditions>
        <comment>{JIRA}: CreateTable MACHINE_CLASS_MEASURE_TIME</comment>
        <createTable tablespace="MW_ENT" remarks="The measure time at a scale for a specific machine class" tableName="MACHINE_CLASS_MEASURE_TIME">
            <column name="OID" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="MEASURE_TIME" type="DOUBLE">
                <constraints nullable="false"/>
            </column>
            <column name="MACHINE_CLASS_OID" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <addPrimaryKey columnNames="OID,MACHINE_CLASS_OID" constraintName="MACHINE_CLASS_MEASURE_TIME_PK" tablespace="MW_IDXENT" tableName="MACHINE_CLASS_MEASURE_TIME"/>
        <createIndex unique="false" tablespace="MW_IDXENT" indexName="MACHINE_CLASS_MEASURE_TIME_12U" tableName="MACHINE_CLASS_MEASURE_TIME">
            <column name="OID"/>
        </createIndex>
        <modifySql dbms="mssql,h2,postgresql">
            <regExpReplace replace="( USING INDEX)? (ON|TABLESPACE) MW_[A-Z]+" with=""/>
        </modifySql>
    </changeSet>
