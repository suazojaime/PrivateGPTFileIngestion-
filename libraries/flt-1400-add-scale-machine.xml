            <tableExists tableName="MACHINE_CLASS_MEASURE_TIME"/>
            <tableExists tableName="MACHINE"/>
            <not>
                <foreignKeyConstraintExists foreignKeyTableName="MACHINE_CLASS_MEASURE_TIME" foreignKeyName="MACHINE_CLASS_MEASURE_TIME_FK1"/>
            </not>
        </preConditions>
        <comment>{JIRA}: AddForeignKeyConstraint MACHINE_CLASS_MEASURE_TIME_FK1 to MACHINE_CLASS_MEASURE_TIME (OID)</comment>
        <sql>DELETE FROM MACHINE_CLASS_MEASURE_TIME WHERE OID IS NOT NULL AND NOT EXISTS (SELECT 'x' FROM MACHINE WHERE MACHINE.MACHINE_OID = MACHINE_CLASS_MEASURE_TIME.OID)</sql>
        <addForeignKeyConstraint baseTableName="MACHINE_CLASS_MEASURE_TIME" baseColumnNames="OID" constraintName="MACHINE_CLASS_MEASURE_TIME_FK1" referencedTableName="MACHINE" referencedColumnNames="MACHINE_OID" onDelete="NO ACTION"/>
        <rollback>
            <dropForeignKeyConstraint baseTableName="MACHINE_CLASS_MEASURE_TIME" constraintName="MACHINE_CLASS_MEASURE_TIME_FK1"/>
        </rollback>
    </changeSet>
