<?xml version="1.0" encoding="us-ascii" standalone="no"?>
<!DOCTYPE eventdefs SYSTEM "eventdefs.dtd">

<eventdefs>
    <eventdef name="TOPE.ARRIVE">
        <pubsubevent>
            <matching>
                <eventdomaintype domain="Minestar" type="VIMS"/>
                <namevaluepairs>{event="arrive"}</namevaluepairs>
            </matching>
        </pubsubevent>
    </eventdef>
    <eventdef name="TOPE.LOAD">
        <pubsubevent>
            <matching>
                <eventdomaintype domain="Minestar" type="VIMS"/>
                <namevaluepairs>{event="load"}</namevaluepairs>
            </matching>
        </pubsubevent>
    </eventdef>
</eventdefs>
