<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:widget="http://www.minestar.au.cat.com/namespace/widget"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.minestar.au.cat.com/namespace/widget http://www.minestar.au.cat.com/namespace/widget.xsd
       ">

    <!-- Shift tonnage by machine class tables -->
    <widget:tableChart id="truckClassShiftTonnageTable" dataSet="truckClassesDataSet" dataSource="truckClassShiftTonnage"
                       domain="properties"
                       labelX="Truck Class" labelY="Tonnes"
                       width="150" height="100"/>

    <widget:tableChart id="lhdClassShiftTonnageTable" dataSet="loadHaulDumpClassesDataSet" dataSource="lhdClassShiftTonnage"
                       domain="properties"
                       labelX="LHD Class" labelY="Tonnes"
                       width="150" height="100"/>

    <!-- Shift tonnage by fleet tables -->
    <widget:tableChart id="truckFleetShiftTonnageTable" dataSet="truckFleetsDataSet" dataSource="truckFleetShiftTonnage"
                       domain="properties"
                       labelX="Fleet" labelY="Tonnes"
                       width="250" height="100"/>

    <widget:tableChart id="lhdFleetShiftTonnageTable" dataSet="loadHaulDumpFleetsDataSet" dataSource="lhdFleetShiftTonnage"
                       domain="properties"
                       labelX="Fleet" labelY="Tonnes"
                       width="250" height="100"/>

    <!-- Availability by machine class tables -->
    <widget:tableChart id="truckClassAvailabilityTable" dataSet="truckClassesDataSet" dataSource="truckClassAvailability"
                       domain="properties"
                       labelX="Truck Class" labelY="Availability"
                       width="150" height="100"/>

    <widget:tableChart id="lhdClassAvailabilityTable" dataSet="loadHaulDumpClassesDataSet" dataSource="lhdClassAvailability"
                       domain="properties"
                       labelX="LHD Class" labelY="Availability"
                       width="150" height="100"/>

    <!-- Availability by fleet tables -->
    <widget:tableChart id="truckFleetAvailabilityTable" dataSet="truckFleetsDataSet" dataSource="truckFleetAvailability"
                       domain="properties"
                       labelX="Fleet" labelY="Availability"
                       width="250" height="100"/>

    <widget:tableChart id="lhdFleetAvailabilityTable" dataSet="loadHaulDumpFleetsDataSet" dataSource="lhdFleetAvailability"
                       domain="properties"
                       labelX="Fleet" labelY="Availability"
                       width="250" height="100"/>

    <!-- Use of availability by machine class tables -->
    <widget:tableChart id="truckClassUseOfAvailabilityTable" dataSet="truckClassesDataSet"
                       dataSource="truckClassUseOfAvailability" domain="properties"
                       labelX="Truck Class" labelY="Use of Availability"
                       width="200" height="100"/>

    <widget:tableChart id="lhdClassUseOfAvailabilityTable" dataSet="loadHaulDumpClassesDataSet"
                       dataSource="lhdClassUseOfAvailability" domain="properties"
                       labelX="LHD Class" labelY="Use of Availability"
                       width="200" height="100"/>

    <!-- Use of availability by fleet tables -->
    <widget:tableChart id="truckFleetUseOfAvailabilityTable" dataSet="truckFleetsDataSet"
                       dataSource="truckFleetUseOfAvailability" domain="properties"
                       labelX="Fleet" labelY="Use of Availability"
                       width="250" height="100"/>

    <widget:tableChart id="lhdFleetUseOfAvailabilityTable" dataSet="loadHaulDumpFleetsDataSet"
                       dataSource="lhdFleetUseOfAvailability" domain="properties"
                       labelX="Fleet" labelY="Use of Availability"
                       width="250" height="100"/>

    <!-- Tons per operating hour by machine class tables -->
    <widget:tableChart id="truckClassTonsPerOperHourTable" dataSet="truckClassesDataSet"
                       dataSource="truckClassTonsPerOperHour" domain="properties"
                       labelX="Truck Class" labelY="Tonnes per Oper. Hour"
                       width="250" height="100"/>

    <widget:tableChart id="lhdClassTonsPerOperHourTable" dataSet="loadHaulDumpClassesDataSet"
                       dataSource="lhdClassTonsPerOperHour" domain="properties"
                       labelX="LHD Class" labelY="Tonnes per Oper. Hour"
                       width="250" height="100"/>

    <!-- Tons per operating hour by fleet tables -->
    <widget:tableChart id="truckFleetTonsPerOperHourTable" dataSet="truckFleetsDataSet"
                       dataSource="truckFleetTonsPerOperHour" domain="properties"
                       labelX="Fleet" labelY="Tonnes per Oper. Hour"
                       width="250" height="100"/>

    <widget:tableChart id="lhdFleetTonsPerOperHourTable" dataSet="loadHaulDumpFleetsDataSet"
                       dataSource="lhdFleetTonsPerOperHour" domain="properties"
                       labelX="Fleet" labelY="Tonnes per Oper. Hour"
                       width="250" height="100"/>

    <!-- Draw card compliance -->
    <widget:tableChart id="drawPointDrawCardComplianceTable" dataSet="drawCardDrawPointDataSet"
                       dataSource="drawCardCompliance" domain="properties"
                       labelX="Draw Point" labelY="Compliance"
                       width="500" height="500"/>
</beans>
