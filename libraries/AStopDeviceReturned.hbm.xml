<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="historical">

  <class name="minestar.mine.domain.minemodel.astop.AStopDeviceReturnedImpl"
         table="ASTOP_DEVICE_RETURNED"
         dynamic-update="true"
         proxy="minestar.mine.domain.minemodel.astop.AStopDeviceReturned">

    <meta attribute="table">
      tablespace=MW_ENT
    </meta>
    <meta attribute="primary-key">
      tablespace=MW_IDXENT
    </meta>

    <meta attribute="description">Records return of AStop device to mine entrants.</meta>

    <id name="OID"
        type="java.lang.Long"
        column="OID">
        <generator class="com.mincom.env.base.persistence.IDGenerator"/>
    </id>

    <property
            name="autStopID"
            type="java.lang.Long"
            column="AUT_STOP_ID">

    </property>

    <property
            name="returnedTime"
            type="TimestampType"
            >
        <meta attribute="view-localtime">true</meta>
        <meta attribute="utc-description">The return utc time.</meta>
        <meta attribute="description">The return time.</meta>
        <column name="RETURNED_TIME_UTC"/>

    </property>

    <property
        name="tester"
        type="java.lang.String"
        column="TESTER">
    </property>

    <property
        name="testStationName"
        type="java.lang.String"
        column="TEST_STATION_NAME">
    </property>


    <property
        name="deviceHolder"
        type="java.lang.String"
        column="DEVICE_HOLDER">
        <meta attribute="description">The mine entrant.</meta>
    </property>

    <property
        name="deployedTime"
        type="TimestampType"
        >
      <meta attribute="utc-description">The utc time when the A-Stop device is lent out.</meta>
      <meta attribute="description">The time when the A-Stop device is lent out.</meta>
      <meta attribute="view-localtime">true</meta>
      <column name="DEPLOYED_TIME_UTC"/>
    </property>

    <property
        name="testTime"
        type="TimestampType"
        >
      <meta attribute="utc-description">The utc time when the A-Stop device is tested.</meta>
      <meta attribute="description">The time when the A-Stop device is tested.</meta>
      <meta attribute="view-localtime">true</meta>
      <column name="TEST_TIME_UTC"/>
    </property>
  </class>
</hibernate-mapping>
