<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="model">

    <class
            name="minestar.external.domain.QrtzSchedulerState"
            table="QRTZ_SCHEDULER_STATE"
    >
        <meta attribute="table">
            tablespace=MW_OBJ
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXOBJ
        </meta>
        <meta attribute="description">The Quartz scheduler state.</meta>

        <id
                name="instanceName"
                type="java.lang.String"
                column="INSTANCE_NAME"
        >
            <meta attribute="description">The instance name.</meta>
            <generator class="assigned"/>
        </id>

        <property
                name="lastCheckinTime"
                type="long"
                column="LAST_CHECKIN_TIME"
                not-null="true"
                length="13"
        >
            <meta attribute="description">The Quartz scheduler last checkin epoc time.</meta>
        </property>
        <property
                name="checkinInterval"
                type="long"
                column="CHECKIN_INTERVAL"
                not-null="true"
                length="13"
        >
            <meta attribute="description">The Quartz scheduler checkin interval.</meta>
        </property>
        <property
                name="recoverer"
                type="java.lang.String"
                column="RECOVERER"
                length="254"
        >
            <meta attribute="description">The Quartz scheduler recoverer.</meta>
        </property>
        <property name="schedName" type="java.lang.String">
            <meta attribute="description">The Scheduler name.</meta>
            <column name="SCHED_NAME" length="120" not-null="true" default="Sched1" />
        </property>
        <!-- associations -->

    </class>
</hibernate-mapping>
