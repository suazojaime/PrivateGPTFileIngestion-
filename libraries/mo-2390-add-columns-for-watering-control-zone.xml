    <changeSet id="ddc30f53-9126-455d-b71b-3379fa9bc016" author="MineStar" failOnError="false">
        <preConditions onFailMessage="Ignored AddColumn for WATERING_FACTOR to table ZONE as table does not exist or column already exists." onFail="MARK_RAN">
