    <DataSet name="MessageInformation" description="Standard Messages" schema="model">
        <EntryPoint name="Message Groups" class="minestar.pitlink.domain.MessageGroup"/>
        <EntryPoint name="Standard Messages" class="minestar.pitlink.domain.StandardMessage"/>
    </DataSet>

    <DataSet name="MineLayout" description="Waypoints, Roads, Locations" schema="model">
        <EntryPoint name="Waypoints" class="minestar.mine.domain.minemodel.WaypointImpl"/>
        <EntryPoint name="Road Segments" class="minestar.mine.domain.minemodel.RoadSegmentImpl"/>
        <EntryPoint name="Locations" class="minestar.mine.domain.minemodel.LocationImpl"/>
        <EntryPoint name="Road Travel Conditions" class="minestar.mine.domain.RoadTravelCondition"/>
    </DataSet>

    <DataSet name="MaterialInformation" description="Materials, Grades, Mining Blocks" schema="model">
        <EntryPoint name="Material Groups" class="minestar.machinetracking.domain.MaterialGroupImpl"/>
        <EntryPoint name="Materials" class="minestar.machinetracking.domain.MaterialImpl"/>
        <EntryPoint name="Material Mixes" class="minestar.machinetracking.domain.MaterialMix"/>
        <EntryPoint name="Discrete Grades" class="minestar.production.domain.grade.DiscreteGrade"/>
        <EntryPoint name="Continuous Grades" class="minestar.production.domain.grade.ContinuousGrade"/>
        <EntryPoint name="Mining Block Groups" class="minestar.mine.domain.material.GradeBlockGroupImpl"/>
        <EntryPoint name="Mining Blocks" class="minestar.mine.domain.material.GradeBlockImpl"/>
        <EntryPoint name="Blends" class="minestar.mine.domain.material.Blend"/>
    </DataSet>

    <DataSet name="ProductionInformation" description="Activities, Delay Classes, Job Codes, Production Request Templates" schema="model">
        <EntryPoint name="Activity Groups" class="minestar.production.domain.ActivityGroup"/>
        <EntryPoint name="Activities" class="minestar.production.domain.Activity"/>
        <EntryPoint name="Delay Classes" class="minestar.production.domain.DelayClass"/>
        <EntryPoint name="Delay Groups" class="minestar.production.domain.DelayGroup"/>
        <EntryPoint name="Job Code Classes" class="minestar.production.domain.JobCodeClass"/>
        <EntryPoint name="Job Code Groups" class="minestar.production.domain.JobCodeGroup"/>
        <EntryPoint name="Job Codes" class="minestar.production.domain.JobCode"/>
        <EntryPoint name="Production Request Templates" class="minestar.assignment.domain.planning.ProductionRequestTemplateImpl"/>
    </DataSet>

    <DataSet name="AssignmentInformation" description="Scheduled Assignments, Assignment Groups" schema="model">
        <!-- scheduled assignment exports can't work because of the broken data modelling -->
        <EntryPoint name="Assignment Groups" class="minestar.assignment.domain.AssGroup"/>
    </DataSet>

    <DataSet name="UndergroundInformation"
             description="Map details required for synchronising Fleet for Underground systems"
             schema="model">
        <EntryPoint name="Waypoints"
                    class="minestar.mine.domain.minemodel.WaypointImpl"
                    filter="IS_ACTIVE = '1'"/>
        <EntryPoint name="RFRNodes"
                    class="minestar.machinetracking.domain.sensor.RFRNodeImpl"
                    filter="IS_ACTIVE = '1'"/>
        <EntryPoint name="PositionOfInterests"
                    class="minestar.mine.domain.minemodel.PositionOfInterestImpl"
                    filter="IS_ACTIVE = '1'"/>
    </DataSet>

    <DataSet name ="AllModelEntities" description="Everything in the Mine Model" schema="model">
        <DataSet name="AlarmInformation"/>
        <DataSet name="Users"/>
        <DataSet name="PersonnelInformation"/>
        <DataSet name="MachineInformation"/>
        <DataSet name="MessageInformation"/>
        <DataSet name="MineLayout"/>
        <DataSet name="MaterialInformation"/>
        <DataSet name="ProductionInformation"/>
        <DataSet name="AssignmentInformation"/>
    </DataSet>

</DataSets>
