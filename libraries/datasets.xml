<!DOCTYPE DataSets SYSTEM "DataSets.dtd">
<DataSets>
    <DataSet name="AlarmInformation" description="Alarm Type Information" schema="model">
        <EntryPoint name="Alarm Types" class="minestar.platform.domain.alarm.AlarmType"/>
    </DataSet>

    <DataSet name="Users" description="MineStar Users and Login Information" schema="model">
        <EntryPoint name="Users" class="minestar.platform.domain.user.MineStarUser"/>
    </DataSet>

    <DataSet name="PersonnelInformation" description="Personnel, Licenses, Crews, Shifts" schema="model">
        <EntryPoint name="Crews" class="minestar.platform.domain.roster.CrewImpl"/>
        <EntryPoint name="Shifts" class="minestar.platform.domain.calendar.Shift"/>
        <EntryPoint name="Absence Types" class="minestar.platform.domain.calendar.AbsenceType"/>
        <EntryPoint name="Work Groups" class="minestar.machinetracking.domain.WorkGroupImpl"/>
        <EntryPoint name="People" class="minestar.machinetracking.domain.PersonImpl"/>
