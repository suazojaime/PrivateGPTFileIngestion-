<?xml version="1.0" standalone="no"?>
<!DOCTYPE WorkOrder_001 SYSTEM "WorkOrder_001.dtd">
<WorkOrder_001>
   <ControlArea>
      <BusinessDocument>
         <BusinessDocType>WorkOrder</BusinessDocType>
         <Version>001</Version>
      </BusinessDocument>
      <DocReferenceID>@@refid@@</DocReferenceID>
      <Confirmation>0</Confirmation>
      <Language>en</Language>
      <Originator>
         <B2BParty>
            <Organisation>
               <Identifier/>
               <AssignedBy/>
               <Name/>
            </Organisation>
         </B2BParty>
      </Originator>
      <Receiver>
         <B2BParty>
            <Organisation>
               <Identifier/>
               <AssignedBy/>
               <Name/>
            </Organisation>
         </B2BParty>
      </Receiver>
      <CreationDate>
         <DateTime>
            <Year>@@date{date,yyyy}@@</Year>
            <Month>@@date{date,MM}@@</Month>
            <Day>@@date{date,dd}@@</Day>
            <Hour>@@date{date,kk}@@</Hour>
            <Minute>@@date{date,mm}@@</Minute>
            <Second>@@date{date,ss}@@</Second>
            <SubSecond>0000</SubSecond>
            <TimeZone>+1000</TimeZone>
         </DateTime>
      </CreationDate>
   </ControlArea>
   <DataArea>
      <WorkOrderCreate>
         <Description>MineStar Test WorkOrder</Description>
         <District>
            <DistrictCode>@@districtCode@@</DistrictCode>
         </District>
         <WorkOrderCreateHeader>
            <WorkOrderClassificationDetails>
               <WorkOrderType>
                  <WOTypeCode>@@woTypeCode@@</WOTypeCode>
               </WorkOrderType>
               <MaintenanceType>
                  <MaintenanceTypeCode>@@maintenanceTypeCode@@</MaintenanceTypeCode>
               </MaintenanceType>
               <WorkOrderJobCode>
                  <WOJobCode>@@woJobCode@@</WOJobCode>
               </WorkOrderJobCode>
            </WorkOrderClassificationDetails>
            <OriginatingDetails>
               <OriginatedBy>
                  <Person>
                     <PersonID>@@originatedBy@@</PersonID>
                  </Person>
               </OriginatedBy>
               <OriginatorsPriority>
                  <PriorityCode>@@priorityCode@@</PriorityCode>
               </OriginatorsPriority>
            </OriginatingDetails>
            <EquipmentWorkDetails>
               <EquipmentID>
                  <EquipmentNumber>@@equipmentNumber@@</EquipmentNumber>
               </EquipmentID>
            </EquipmentWorkDetails>
            <AssignTo>
               <WorkGroup>
                  <WorkGroupID>@@workGroupId@@</WorkGroupID>
               </WorkGroup>
            </AssignTo>
         </WorkOrderCreateHeader>
         <WorkOrderCreateTaskList>
            <WorkOrderCreateTask>
               <WOTaskDescription>@@taskDescription@@</WOTaskDescription>
               <AssignTo>
                  <AssignToPerson>
                     <Person>
                        <PersonID>@@assignTo@@</PersonID>
                     </Person>
                  </AssignToPerson>
                  <WorkGroup>
                     <WorkGroupID>@@workGroupId@@</WorkGroupID>
                  </WorkGroup>
               </AssignTo>
            </WorkOrderCreateTask>
         </WorkOrderCreateTaskList>
      </WorkOrderCreate>
   </DataArea>
</WorkOrder_001>

