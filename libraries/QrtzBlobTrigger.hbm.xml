<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="model">

    <class
            name="minestar.external.domain.QrtzBlobTrigger"
            table="QRTZ_BLOB_TRIGGERS"
    >
        <meta attribute="table">
            tablespace=MW_OBJ
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXOBJ
        </meta>
        <meta attribute="description">The Quartz Blob trigger.</meta>

        <composite-id name="comp_id" class="minestar.external.domain.QrtzBlobTriggerPK">
            <!-- bi-directional one-to-one association to QrtzTrigger -->
            <key-many-to-one
                    name="qrtzTrigger"
                    class="minestar.external.domain.QrtzTrigger"
            >
                <meta attribute="use-in-equals">true</meta>
                <column name="TRIGGER_NAME"/>
                <column name="TRIGGER_GROUP"/>
            </key-many-to-one>
        </composite-id>
        <!-- Data type changed from java.lang.long to java.sql.Blob : MSTAR:7000 Nags -->
        <property
                name="blobData"
                type="java.sql.Blob"
                column="BLOB_DATA"
        >
            <meta attribute="description">The serialized data.</meta>
        </property>
        <property name="schedName" type="java.lang.String">
            <meta attribute="description">The Scheduler name.</meta>
            <column name="SCHED_NAME" length="120" not-null="true" default="Sched1" />
        </property>

        <!-- associations -->
        <!-- bi-directional one-to-one association to QrtzTrigger -->
        <one-to-one
                name="qrtzTrigger"
                class="minestar.external.domain.QrtzTrigger"
                outer-join="auto"
                constrained="true"
        >
        </one-to-one>

    </class>
</hibernate-mapping>
