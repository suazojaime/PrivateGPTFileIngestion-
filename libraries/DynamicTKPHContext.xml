<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="dynamicTkphUCCImpl" class="minestar.mine.uifacade.dynamictkph.impl.DynamicTkphUCCImpl"
          lazy-init="true" init-method="init">
        <constructor-arg name="machineTkphService" ref="machineTkphServiceImpl"/>
        <constructor-arg name="notification" ref="notification"/>
    </bean>

    <bean id="registerDynamicTkphUCCImpl" parent="registrationInstance" lazy-init="true">
        <constructor-arg value="Dynamic TKPH UCC"/>
        <constructor-arg value="minestar.mine.uifacade.dynamictkph.DynamicTkphUCCFacade"/>
        <constructor-arg ref="dynamicTkphUCCImpl"/>
    </bean>
</beans>
