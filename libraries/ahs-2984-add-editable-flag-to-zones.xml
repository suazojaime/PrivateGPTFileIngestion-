<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="23755857-bf67-471f-ba9e-75da56212b8d" author="MineStar" failOnError="false">
        <preConditions onFailMessage="Ignored AddColumn for EDITABLE to table ZONE as table does not exist or column already exists." onFail="MARK_RAN">
            <tableExists tableName="ZONE"/>
            <not>
                <columnExists tableName="ZONE" columnName="EDITABLE"/>
            </not>
        </preConditions>
        <comment>AHS-2984: AddColumn ZONE.EDITABLE</comment>
        <addColumn tableName="ZONE">
            <column name="EDITABLE" type="BOOLEAN" defaultValueBoolean="true" remarks="Is this zone editable?">
                <constraints nullable="false"/>
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>
