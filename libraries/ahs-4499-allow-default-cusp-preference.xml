        <preConditions onFailMessage="Ignored AddColumn for ENFORCE_CUSP_PREFERENCE to table PLAN_MODEL as table does not exist or column already exists." onFail="MARK_RAN">
            <tableExists tableName="PLAN_MODEL"/>
            <not>
                <columnExists tableName="PLAN_MODEL" columnName="ENFORCE_CUSP_PREFERENCE"/>
            </not>
        </preConditions>
        <comment>AHS-4499 : AddColumn PLAN_MODEL.ENFORCE_CUSP_PREFERENCE</comment>
        <addColumn tableName="PLAN_MODEL">
            <column name="ENFORCE_CUSP_PREFERENCE" type="BOOLEAN" defaultValueBoolean="false"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>
