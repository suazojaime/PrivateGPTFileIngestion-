<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="model">

    <class
            name="minestar.platform.domain.lookup.TimezoneDST"
            table="DST_TIMEZONE_CONFIG"
            >
        <meta attribute="table">
            tablespace=MW_ECF
        </meta>
        <meta attribute="primary-key">
            tablespace=MW_IDXENT
        </meta>

        <id name="ID"
            type="java.lang.Integer"
            column="DST_TIMEZONE_ID">
            <generator class="native"/>
        </id>
        <property
                name="TimeZone"
                column="TIMEZONE"
                type="java.lang.String"
                length="80">
        </property>
        <property
                name="DstYear"
                column="DST_YEAR"
                type="java.lang.Long">
        </property>
        <property
                name="DstCutOver"
                type="java.util.Date"
                column="DST_CUTOVER"
                >
            <meta attribute="timestamp">true</meta>
        </property>
        <property
                name="DstCutBack"
                type="java.util.Date"
                column="DST_CUTBACK"
                >
            <meta attribute="timestamp">true</meta>
        </property>
        <property
                name="TimezoneOffset"
                column="TIMEZONE_OFFSET"
                type="java.lang.Long">
        </property>
        <property
                name="DstOffset"
                column="DST_OFFSET"
                type="java.lang.Long">
        </property>
    </class>
</hibernate-mapping>
