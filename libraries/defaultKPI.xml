<?xml version="1.0" encoding="us-ascii" standalone="no"?>
<!DOCTYPE onboardKPIWindows SYSTEM "onboardKPIs.dtd">
<!-- This is the default KPI Defination for sending Destination Message and should not be removed.
This is used for sending destination Messages to Loader,Shovel as Loading Status -->
<onboardKPIWindows>
 <onboardKPIWindow name="Loading Status " window="1" color="0" displayPercent="false" displayTarget="false">
        <source name="NotifyLoadingTool" minBetween="120"/>
        <machineCategory name="Loader Classes" target="30"/>
        <machineCategory name="Shovel Classes" target="30"/>
        <machineCategory name="Surface Miner Classes" target="1"/>
    </onboardKPIWindow>
</onboardKPIWindows>
