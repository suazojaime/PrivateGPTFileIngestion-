<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       ">

    <bean id="roadChangesServiceImpl" class="minestar.pitlink.service.pitmodel.RoadChangesServiceImpl" init-method="init">
        <constructor-arg name="notification" ref="notification"/>
        <constructor-arg name="fleetAssignmentService" ref="fleetAssignmentServiceImpl"/>
        <constructor-arg name="pitModelService" ref="pitModelServiceImpl"/>
    </bean>

</beans>
