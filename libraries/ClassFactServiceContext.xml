<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                                                http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="classFactDao" class="minestar.platform.persistence.ClassFactDaoImpl" parent="minestarDao" lazy-init="true" />

    <bean id="classFactServiceImpl" class="minestar.platform.service.generation.ClassFactServiceImpl" lazy-init="true">
        <property name="classFactDao" ref="classFactDao" />
    </bean>

</beans>
